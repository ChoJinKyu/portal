{
  "_version": "1.12.0",
  "sap.app": {
    "id": "xx.templateRcmdV4",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "srv-api/odata/v4/xx.TemplateService/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      },
      "commonService": {
        "uri": "srv-api/odata/v2/cm.util.CommonService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "xx.templateRcmdV4.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "handleValidation": true,
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "xx.templateRcmdV4.i18n.i18n"
        }
      },
      "common": {
        "dataSource": "commonService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": true
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "groupId": "$direct",
          "synchronizationMode": "None",
          "autoExpandSelect": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "../../../lib/css/style.css"
        },
        {
          "uri": "../../../lib/css/layout.css"
        },
        {
          "uri": "css/style.css"
        }
      ]
    },
    "resourceRoots": {
      "ext.lib": "../../../lib",
      "cm.util": "../../../cm/util"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "xx.templateRcmdV4.view",
        "controlId": "fcl",
        "transition": "slide",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": ":layout:",
          "name": "beginPage",
          "target": [
            "beginColumn"
          ]
        },
        {
          "pattern": "midColumn/{layout}/{tenantId}^{companyCode}",
          "name": "midPage",
          "target": [
            "beginColumn",
            "midColumn"
          ]
        },
        {
          "pattern": "endColumn/{layout}/{tenantId}^{companyCode}^{departmentCode}",
          "name": "endPage",
          "target": [
            "beginColumn",
            "midColumn",
            "endColumn"
          ]
        }
      ],
      "targets": {
        "beginColumn": {
          "viewName": "TemplateBeginColumn",
          "controlAggregation": "beginColumnPages"
        },
        "midColumn": {
          "viewName": "TemplateMidColumn",
          "controlAggregation": "midColumnPages"
        },
        "endColumn": {
          "viewName": "TemplateEndColumn",
          "controlAggregation": "endColumnPages"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewPath": "ext.lib.view",
          "viewId": "notFound"
        }
      }
    }
  }
}