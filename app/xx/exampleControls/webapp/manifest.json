{
  "_version": "1.12.0",
  "sap.app": {
    "id": "xx.exampleControls",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "srv-api/odata/v2/xx.TemplateService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "commonService": {
        "uri": "srv-api/odata/v2/cm.util.CommonService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "orgService": {
        "uri": "srv-api/odata/v2/cm.util.OrgService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "xx.exampleControls.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "handleValidation": true,
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "xx.exampleControls.i18n.i18n"
        }
      },
      "common": {
        "dataSource": "commonService",
        "type": "ext.lib.model.v2.ODataModel",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": true
        }
      },
      "org": {
        "dataSource": "orgService",
        "type": "ext.lib.model.v2.ODataModel",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": true
        }
      },
      "odata": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": true,
          "useBatch": true
        }
      }
    },
    "resources": {
        "css": [
            {
                "uri": "css/style.css"
            }
        ]
    },
    "resourceRoots": {
        "ext.lib": "../../../lib"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "xx.exampleControls.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": [
            "home"
          ]
        },
        {
          "pattern": "example1",
          "name": "example1",
          "target": [
            "example1"
          ]
        },
        {
          "pattern": "example2",
          "name": "example2",
          "target": [
            "example2"
          ]
        },
        {
          "pattern": "example3",
          "name": "example3",
          "target": [
            "example3"
          ]
        },
        {
          "pattern": "example4",
          "name": "example4",
          "target": [
            "example4"
          ]
        },
        {
          "pattern": "example5",
          "name": "example5",
          "target": [
            "example5"
          ]
        },
        {
          "pattern": "example6",
          "name": "example6",
          "target": [
            "example6"
          ]
        },
        {
          "pattern": "example7",
          "name": "example7",
          "target": [
            "example7"
          ]
        },
        {
          "pattern": "example8",
          "name": "example8",
          "target": [
            "example8"
          ]
        }
      ],
      "targets": {
        "home": {
          "viewName": "Home",
          "viewId": "home"
        },
        "example1": {
          "viewName": "SearchAreaControls",
          "viewId": "example1"
        },
        "example2": {
          "viewName": "ControlValidation",
          "viewId": "example2"
        },
        "example3": {
          "viewName": "SmartTable",
          "viewId": "example3"
        },
        "example4": {
          "viewName": "InlineEditTable",
          "viewId": "example4"
        },
        "example5": {
          "viewName": "CodeCombo",
          "viewId": "example5"
        },
        "example6": {
          "viewName": "ExcelUtils",
          "viewId": "example6"
        },
        "example7": {
          "viewName": "PivotTable",
          "viewId": "example7"
        },
        "example8": {
          "viewName": "NumberFormat",
          "viewId": "example8"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewPath": "ext.lib.view",
          "viewId": "notFound"
        }
      }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "sppCap_ui_dev"
  }
}