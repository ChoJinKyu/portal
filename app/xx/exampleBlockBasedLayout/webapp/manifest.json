{
  "_version": "1.12.0",
  "sap.app": {
    "id": "xx.exampleBlockBasedLayout",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach",
    "resources": "resources.json",
    "dataSources": {}
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "xx.exampleBlockBasedLayout.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "handleValidation": true,
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "xx.exampleBlockBasedLayout.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "../../../lib/css/style.css"
        },
        {
          "uri": "../../../lib/css/layout.css"
        },
        {
          "uri": "css/style.css"
        }
      ]
    },
    "resourceRoots": {
      "ext.lib": "../../../lib",
      "cm.util": "../../../cm/util"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "xx.exampleBlockBasedLayout.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "home",
          "target": [
            "home"
          ]
        },
        {
          "pattern": "example1",
          "name": "example1",
          "target": [
            "example1"
          ]
        },
        {
          "pattern": "example2",
          "name": "example2",
          "target": [
            "example2"
          ]
        }
      ],
      "targets": {
        "home": {
          "viewName": "Home",
          "viewId": "home"
        },
        "example1": {
          "viewName": "BlockBasePage1",
          "viewId": "example1"
        },
        "example2": {
          "viewName": "BlockBasePage2",
          "viewId": "example2"
        }
      }
    }
  }
}