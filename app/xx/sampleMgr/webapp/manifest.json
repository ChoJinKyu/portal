{
    "_version": "1.12.0",
    "sap.app": {
        "id": "xx.sampleMgr",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/xx.SampleMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "v4Proc": {
                "uri": "srv-api/odata/v4/xx.SampleMgrV4Service/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sppCap_ui_dev"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "xx.sampleMgr.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "xx.sampleMgr.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true
                }
            },
            "v4Proc": {
                "dataSource": "v4Proc",
                "preload": false,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": false
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "resourceRoots": {
            "ext.lib": "../../../lib"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "xx.sampleMgr.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                }
            },
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "mainList",
                    "target": [
                        "mainList"
                    ]
                }
            ],
            "targets": {
                "mainList": {
                    "viewName": "MainList",
                    "viewId": "mainList",
                    "viewLevel": 1
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewPath": "ext.lib.view",
                    "viewId": "notFound"
                }
            }
        }
    }
}