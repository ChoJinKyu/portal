<mvc:View displayBlock="true" controllerName="com.template.sampleFlexibleColumnTemplate.controller.FlexibleColumnLayout" height="100%"
	  xmlns="sap.f"
	  xmlns:mvc="sap.ui.core.mvc">
	<FlexibleColumnLayout
	    columnResize="onColumnResize"
		autoFocus="false"
		id="fcl"
		stateChange="onStateChanged"
		restoreFocusOnBackNavigation="true"
		layout="{/layout}"
		backgroundDesign="Translucent"/>
</mvc:View>
