<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core" 
    xmlns:f="sap.f"
    xmlns:uxap="sap.uxap" 
    xmlns:form="sap.ui.layout.form"
    xmlns:layout="sap.ui.layout">
    <Dialog id="dialogForexChart" class="sapUiSmallMargin" title="외화신고대상 차트" contentWidth="35%" afterClose="dialogAfterclose">
        <VBox class="sapUiSmallMargin">
            <form:SimpleForm
                editable="false"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="3"
                labelSpanM="4"
                labelSpanS="12"
                adjustLabelSpan="false"
                emptySpanXL="0"
                emptySpanL="4"
                emptySpanM="0"
                emptySpanS="0"
                singleContainerFullSize="false" >
                    
                    <form:content>
                        <VBox>
                            <Label text = "PO번호 : "/> 
                             <Text id="poNumber" width="100%"/> 

                             <layoutData>
                                <layout:GridData span="XL6 L12 M6 S6" />
                            </layoutData> 
                        </VBox>
                        
                        
                       </form:content>
                    
                  </form:SimpleForm>
            </VBox>       
      
         <ToolbarSpacer/>
        <HBox alignItems="Center" justifyContent="Center" >
            <Button text="Cancel" class="sapUiSmallMargin" type="Transparent" press=".onExitSummaryChart"/>
            <!-- <Button text="Cancel Complete" class="sapUiSmallMargin" type="Emphasized" press=".onForexSave('N')"/> -->
        </HBox>

    </Dialog>
</core:FragmentDefinition>
