{
    "_version": "1.12.0",
    "sap.app": {
        "id": "ep.po.loiPublishMgt",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "ach": "set-ach",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/ep.LoiMgtService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "commonService": {
                "uri": "srv-api/odata/v2/cm.util.CommonService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "ep.po.loiPublishMgt.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "handleValidation": true,
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ep.po.loiPublishMgt.i18n.i18n"
                }
            },
            "common": {
                "dataSource": "commonService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": true
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "resourceRoots": {
            "ext.lib": "../../../../lib",
            "sp.util": "../../../../sp/util"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "ep.po.loiPublishMgt.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "mainPage",
                    "target": [
                        "mainObject"
                    ]
                },
                {
                    "pattern": "selectionObject/{tenantId}/{companyCode}/{loiWriteNumber}/{loiItemNumber}/{loiSelectionNumber}/{loiNumber}/{quotationNumber}/{quotationItemNumber}",
                    "name": "selectionPage",
                    "target": [
                        "selectionObject"
                    ]
                },
                {
                    "pattern": "publishObject/{tenantId}/{companyCode}/{loiWriteNumber}/{loiItemNumber}/{loiPublishNumber}/{loiNumber}",
                    "name": "publishPage",
                    "target": [
                        "publishObject"
                    ]
                }
            ],
            "targets": {
                "mainObject": {
                    "viewName": "MainList",
                    "viewId": "mainPage",
                    "viewLevel": 1
                },
                "selectionObject": {
                    "viewName": "SupplySelection",
                    "viewId": "selectionPage",
                    "viewLevel": 2
                },
                "publishObject": {
                    "viewName": "Publish",
                    "viewId": "publishPage",
                    "viewLevel": 2
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewPath": "ext.lib.view",
                    "viewId": "notFound"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sppCap_ui_dev"
    }
}