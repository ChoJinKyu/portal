{
  "_version": "1.12.0",
  "sap.app": {
    "id": "ep.po.loiRequestMgt",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "srv-api/odata/v2/ep.LoiMgtService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "v4Proc": {
                "uri": "srv-api/odata/v4/ep.SampleMgrV4Service/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
        },
      "commonUtilService": {
        "uri": "srv-api/odata/v2/cm.util.CommonService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "treeService": {
        "uri": "srv-api/odata/v2/xx.TestTableService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "multiService": {
        "uri": "srv-api/odata/v2/ep.LoiMgtService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "orgMgtService": {
        "uri": "srv-api/odata/v2/cm.util.OrgService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ep.po.loiRequestMgt.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "handleValidation": true,
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ep.po.loiRequestMgt.i18n.i18n"
        }
      },
      "tree": {
        "dataSource": "treeService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": true
        }
      },
      "util": {
        "dataSource": "commonUtilService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": true
        }
      },
       "multiLoi": {
        "dataSource": "multiService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "groupId": "$direct",
          "synchronizationMode": "None",
          "autoExpandSelect": true
        }
      },
      "org": {
        "dataSource": "orgMgtService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": true,
          "useBatch": true
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": true,
          "useBatch": true
        }
      },
        "v4Proc": {
            "dataSource": "v4Proc",
            "preload": false,
            "settings": {
                "synchronizationMode": "None",
                "operationMode": "Server",
                "autoExpandSelect": false
            }
        }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "resourceRoots": {
      "ext.lib": "../../../../lib",
      "cm": "../../../../cm"
    },
    "routing": {
  

       "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "ep.po.loiRequestMgt.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
 

      
      "routes": [
				{
					"pattern": "",
					"name": "mainPage",
					"target": [
						"mainObject"
					]
				},
				{
					"pattern": "midObject/{layout}/{tenantId}/{companyCode}/{loiWriteNumber}",
					"name": "midPage",
					"target": [
						"midObject"
					]
				}
            ],
        "targets": {

                "mainObject": {
                    "viewName": "MainList",
                    "viewId": "mainPage",
                    "viewLevel": 1
                },
				"midObject": {
                    "viewName": "MidObject",
                    "viewId": "midPage",
                    "viewLevel": 2
				},
				"notFound": {
          "viewName": "NotFound",
          "viewPath": "ext.lib.view",
          "viewId": "notFound"
        }
    }

    }
  },
  "sap.cloud": {
    "public": true,
    "service": "sppCap_ui_dev"
  }
}