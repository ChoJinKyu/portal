{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sp.sc.scQBMgt",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/sp.sourcingService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "buyer": {
                "uri": "srv-api/odata/v2/pg.taskMonitoringService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
	},
	
	"sap.cloud": {
		"public": true,
		"service": "sppCap_ui_dev"
	},
	
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "sp.sc.scQBMgt.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sp.sc.scQBMgt.i18n.i18n"
				}
			},
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "odataVersion": "2.0"
                },
                "dataSource": "mainService"
            },
            "buyer": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "odataVersion": "2.0"
                },
                "dataSource": "buyer"
            }
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
        "resourceRoots": {
            "ext.lib": "../../../../lib",
            "dp.util": "../../../../dp/util",
            "sp.util": "../../../util"
        },
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sp.sc.scQBMgt.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
                    "name": "mainPage",
                    "pattern": "",
                    "target": ["mainObject"]
                }
            
            ],
			"targets": {
				"mainObject": {
					"viewType": "XML",
					"viewName": "MainList"
				}
			}
		}
	}
}
