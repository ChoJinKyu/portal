{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sp.se.evaluationItemMngt",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/sp.evaluationItemMngtService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            
            "commonService": {
                "uri": "srv-api/odata/v2/cm.util.CommonService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
	},
	
	"sap.cloud": {
		"public": true,
		"service": "sppCap_ui_dev"
	},
	
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "sp.se.evaluationItemMngt.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sp.se.evaluationItemMngt.i18n.i18n"
				}
			},
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true
                }
            },
            
            "common" : {
                "dataSource": "commonService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true
                }
                
            },
            "viewModel" : {
                "type" : "sap.ui.model.json.JSONModel"
            }
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
        },
        "resourceRoots": {
            "ext.lib": "../../../../lib"
        },
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sp.se.evaluationItemMngt.view",
                "controlAggregation": "slide",
                "transition": "slide",
				"controlId": "flexibleColumnLayout"
			},
			"routes": [
                {
                    "name": "Detail",
                    "pattern": "Detail/{new}/:level:/:leaf:/:evaluArticleCode:",
                    "target": ["Master", "Detail"]
                },
                {
                    "name": "Master",
                    "pattern": ":search:",
                    "target": ["Master", "Detail"]
                }
            ],
			"targets": {
				"Master": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": true,
					"viewId": "Master",
					"viewName": "Master",
                    "viewLevel": 1,
					"controlAggregation": "beginColumnPages"
				},
				"Detail": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Detail",
					"viewId": "Detail",
                    "viewLevel": 2,
					"clearControlAggregation": true,
					"controlAggregation": "midColumnPages"
                    
				}
			}
		}
	}
}