<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<form:SimpleForm id="midObjectForm1Show" editable="false">

		<core:Title text="Normal" />
		<Label text="Chain"/>
		<Text text="{master>/chain_code}"/>
		<Label text="Code"/>
		<Text text="{master>/control_option_code}"/>
		<Label text="Name"/>
		<Text text="{master>/control_option_name}"/>

		<core:Title text="Effect" />
		<Label text="From"/>
		<Text text="{master>/start_date}"/>
		<Label text="To"/>
		<Text text="{master>/end_date}"/>
		
		<core:Title text="Boundry" />
		<Label text="Site"/>
		<Text text="{master>/site_flag}"></Text>
		<Label text="Company"/>
		<Text text="{master>/company_flag}"></Text>
		<Label text="Organization"/>
		<Text text="{master>/organization_flag}"></Text>
		<Label text="User"/>
		<Text text="{master>/user_flag}"></Text>

	</form:SimpleForm>
</core:FragmentDefinition>