<mvc:View 
    controllerName="cm.currencyMgr.controller.currencyMainView" 
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true" 
    xmlns="sap.m" 
    xmlns:f="sap.f"
    height="100%">
    <App id="app">
            
        <f:FlexibleColumnLayout
            id="flexibleColumnLayout"
            stateChange=".onStateChanged"
            backgroundDesign="Translucent"
            layout="{Currency>/layout}"/>
                  
    </App>
               
				
</mvc:View>
