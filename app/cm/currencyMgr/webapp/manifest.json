{
  "_version": "1.12.0",
  "sap.app": {
    "id": "cm.currencyMgr",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "dataSources": {
      "mainService": {
        "uri": "srv-api/odata/v2/cm.CurrencyMgrService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "sppCap_ui_dev"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "cm.currencyMgr.view.App",
      "type": "XML",
      "async": true,
      "id": "flexibleColumnLayout"
    },
    "dependencies": {
      "minUI5Version": "1.60.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.f": {},
        "sap.uxap": {}
      }
    },
    "config": {
			"fullWidth": true
		},
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "cm.currencyMgr.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "groupId": "$direct",
          "synchronizationMode": "None",
          "autoExpandSelect": true
        }
      }
    },
    "resourceRoots": {
      "ext.lib": "../../../lib"
    },
    "resources": "resources.json",
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "cm.currencyMgr.view",
        "controlId": "flexibleColumnLayout",
        "transition": "slide",
        "bypassed": {
            }
      },
			"routes": [
                
				{
					"pattern": ":layout:",
					"name": "master",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "detail/{currency}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
				}
			],
			"targets": {
				"master": {
					"viewName": "currencyMasterView",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"viewName": "currencyDetailView",
					"controlAggregation": "midColumnPages"
				}
			}
    }
  }
}