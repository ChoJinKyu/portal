{
    "_version": "1.12.0",
    "sap.app": {
        "id": "cm.orgCodeMgr",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/cm.OrgCodeMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "commonUtilService": {
                "uri": "srv-api/odata/v2/cm.util.CommonService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "orgMgrService": {
                "uri": "srv-api/odata/v2/cm.OrgMgrService/",
                "type": "OData",
                "settings": {
                "odataVersion": "2.0",
                "localUri": "localService/metadata.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "intent1": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "OrgCodeMgr",
                    "action": "display",
                    "title": "{{appTitle}}"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sppCap_ui_dev"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "config" : {"fullWidth": true},
        "flexEnabled": false,
        "handleValidation": true,
        "rootView": {
            "viewName": "cm.orgCodeMgr.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.60.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cm.codeManagement.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "type":"sap.ui.model.odata.v2.ODataModel",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "useBatch" : false,
                    "synchronizationMode": "None",
                    "autoExpandSelect": true,
                    "defaultBindingMode": "TwoWay"
                }
            },
            "util": {
                "dataSource": "commonUtilService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": false
                }
            },
            "orgMgr": {
                "dataSource": "orgMgrService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": false
                }
            },
            "contModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "viewModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            }
        },
        "resourceRoots": {
            "ext.lib": "../../../lib"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "cm.orgCodeMgr.view",
                "controlId": "fcl"
            },

            "routes": [
				{
					"pattern": ":layout:",
					"name": "master",
					"target": [
						"master"
					]
				},
				{
					"pattern": "detail/{layout}/:tenantId:/:groupCode:",
					"name": "detail",
					"target": [
                        "master",
						"detail"
					]
				},
				{
					"pattern": "detailDetail/{layout}/:tenantId:/:groupCode:/:orgCode:/:code:",
					"name": "detailDetail",
					"target": [
						"master",
						"detail",
						"detailDetail"
					]
				}
			],
			"targets": {
				"master": {
                    "viewName": "Master",
                    "controlAggregation": "beginColumnPages"
				},
				"detail": {
                    "viewName": "Detail",
                    "controlAggregation": "midColumnPages"
				},
				"detailDetail": {
                    "viewName": "DetailDetail",
                    "controlAggregation": "endColumnPages"
				}
			}

        }
    }
}