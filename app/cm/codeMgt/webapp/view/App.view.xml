<mvc:View
	controllerName="cm.codeMgt.controller.App"
	displayBlock="true"
	xmlns="sap.m"
    xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">

	<App>
		<f:FlexibleColumnLayout
			id="fcl"
			autoFocus="false"
			restoreFocusOnBackNavigation="true"
			columnResize="onColumnResize"
			layout="{fcl>/layout}"
			backgroundDesign="Translucent"/>
	</App>
</mvc:View>