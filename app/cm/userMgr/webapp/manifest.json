{
    "_version": "1.12.0",
    "sap.app": {
        "id": "cm.userMgr",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/cm.userMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "orgMgrService": {
                "uri": "../../../cm/codeMgr/srv-api/odata/v2/cm.OrgMgrService/",
                "type": "OData",
                "settings": {
                "odataVersion": "2.0",
                "localUri": "localService/metadata.xml"
                }
            },
            "commonUtilService": {
                "uri": "srv-api/odata/v2/cm.util.CommonService/",
                "type": "OData",
                "settings": {
                "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
        "icon": "sap-icon://task",
        "favIcon": "",
        "phone": "",
        "phone@2": "",
        "tablet": "",
        "tablet@2": ""
        },
        "deviceTypes": {
        "desktop": true,
        "tablet": true,
        "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
        "viewName": "cm.userMgr.view.App",
        "type": "XML",
        "async": true,
        "id": "app"
        },
        "dependencies": {
        "minUI5Version": "1.66.0",
        "libs": {
            "sap.ui.core": {},
            "sap.m": {},
            "sap.f": {}
        }
        },
        "contentDensities": {
        "compact": true,
        "cozy": true
        },
        "handleValidation": true,
        "models": {
        "i18n": {
            "type": "sap.ui.model.resource.ResourceModel",
            "settings": {
            "bundleName": "cm.userMgr.i18n.i18n"
            }
        },
        "tree": {
            "dataSource": "treeService",
            "preload": true,
            "settings": {
            "defaultBindingMode": "TwoWay",
            "defaultCountMode": "Inline",
            "refreshAfterChange": false,
            "useBatch": true
            }
        },
        "util": {
            "dataSource": "commonUtilService",
            "preload": true,
            "settings": {
            "defaultBindingMode": "TwoWay",
            "defaultCountMode": "Inline",
            "refreshAfterChange": false,
            "useBatch": true
            }
        },
        "org": {
            "dataSource": "orgMgrService",
            "preload": true,
            "settings": {
            "operationMode": "Server",
            "groupId": "$direct",
            "synchronizationMode": "None",
            "autoExpandSelect": true
            }
        },
        "": {
            "dataSource": "mainService",
            "preload": true,
            "settings": {
            "defaultBindingMode": "TwoWay",
            "defaultCountMode": "Inline",
            "refreshAfterChange": true,
            "useBatch": true
            }
        }
        },
        "resources": {
        "css": [
            {
            "uri": "css/style.css"
            }
        ]
        },
        "resourceRoots": {
        "ext.lib": "../../../lib",
        "3rd.lib": "../../../node_modules"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "cm.userMgr.view",
                "controlId": "fcl"
            },

            "routes": [
                {
                    "pattern": ":layout:",
                    "name": "master",
                    "target": [
                        "master"
                    ]
                },
                {
                    "pattern": "detail/{layout}/:tenantId:/:groupCode:",
                    "name": "detail",
                    "target": [
                        "master",
                        "detail"
                    ]
                },
                {
                    "pattern": "detailDetail/{layout}/:tenantId:/:groupCode:/:code:",
                    "name": "detailDetail",
                    "target": [
                        "master",
                        "detail",
                        "detailDetail"
                    ]
                }
            ],
            "targets": {
                "master": {
                    "viewName": "Master",
                    "controlAggregation": "beginColumnPages"
                },
                "detail": {
                    "viewName": "Detail",
                    "controlAggregation": "midColumnPages"
                },
                "detailDetail": {
                    "viewName": "DetailDetail",
                    "controlAggregation": "endColumnPages"
                }
            }

        }
    }
}