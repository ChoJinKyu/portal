{
    "_version": "1.12.0",
    "sap.app": {
        "id": "cm.userMgr",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "ach": "set-ach",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/cm.UserMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "orgMgrService": {
                "uri": "srv-api/odata/v2/cm.OrgMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "commonUtilService": {
                "uri": "srv-api/odata/v2/cm.util.CommonService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "currencyMgrService": {
                "uri": "srv-api/odata/v2/cm.CurrencyMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "timeZoneMgrService": {
                "uri": "srv-api/odata/v2/cm.TimeZoneMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "cm.userMgr.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "handleValidation": true,
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cm.userMgr.i18n.i18n"
                }
            },
            "util": {
                "dataSource": "commonUtilService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": true
                }
            },
            "org": {
                "dataSource": "orgMgrService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "currency": {
                "dataSource": "currencyMgrService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "timeZoneMgr": {
                "dataSource": "timeZoneMgrService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true
                }
            }
        },
        "resources": {
            "css": [{
                "uri": "css/style.css"
            }]
        },
        "resourceRoots": {
            "ext.lib": "../../../lib",
            "3rd.lib": "../../../node_modules"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "cm.userMgr.view",
                "controlId": "fcl",
                "transition": "slide",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": ":layout:",
                    "name": "mainPage",
                    "target": [
                        "mainObject"
                    ]
                },
                {
                    "pattern": "midObject/{layout}/{userId}/:?query:",
                    "name": "midPage",
                    "target": [
                        "mainObject",
                        "midObject"
                    ]
                }
            ],
            "targets": {
                "mainObject": {
                    "viewName": "MainList",
                    "controlAggregation": "beginColumnPages"
                },
                "midObject": {
                    "viewName": "MidObject",
                    "controlAggregation": "midColumnPages"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewPath": "ext.lib.view",
                    "viewId": "notFound"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sppCap_ui_dev"
    }
}