{
    "_version": "1.21.0",
    "sap.app": {
        "id": "pg.monitor",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/pg.monitorService/",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "orgService": {
                 "uri": "srv-api/odata/v2/cm.OrgMgrService/",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "view": {
                "uri": "monitor.json",
                "type": "JSON"
            },
            "odata": {
                "uri": "tempData.json",
                "type": "JSON"
            },
            "up": {
                "uri": "uploadCollection.json",
                "type": "JSON"
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "pg.monitor.view.Detail",
            "type": "XML",
            "async": true
        },
        "dependencies": {
            "minUI5Version": "1.60.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "pg.monitor.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "odataVersion": "2.0"
                },
                "dataSource": "mainService"
            },
            "orgModel": {
                 "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "odataVersion": "2.0"
                },
                "dataSource": "orgService"
            },
            "view": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "view"
            },
            "odata": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "odata"
            },
            "up": {
                "type": "sap.ui.model.json.JSONModel",
                "dataSource": "up"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "controlId": "app",
                "viewPath": "pg.monitor.view",
                "controlAggregation": "pages",
                "async": true,
                "transition": "slide",
                "bypassed": {
                    "target": "notFound"
                }                
            },
            "routes": [
                {
                    "name": "monitor",
                    "pattern": "monitor",
                    "target": [
                        "monitor"
                    ]
                },
                {
                    "name": "detail",
                    "pattern": "detail/{scenario_code},{tenant_id},{company_code}",
                    "target": [
                        "detail"
                    ]
                }
            ],
            "targets": {
                "monitor": {
                    "viewName": "Monitor",
                    "viewType": "XML",
                    "viewLevel" : 1,
                    "viewId": "monitor"
                },
                "detail": {
                    "viewName": "Detail",
                    "viewType": "XML",
                    "viewLevel" : 2,
                    "viewId": "detail"

                },
                "detailObjectNotFound": {
					"viewName": "DetailObjectNotFound",
					"transition": "show"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "transition": "show"
                }
               
            }
        }
    }
}