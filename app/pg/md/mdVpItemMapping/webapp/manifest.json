{
  "_version": "1.12.0",
  "sap.app": {
    "id": "pg.md.mdVpItemMapping",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "sourceTemplate": {
      "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
      "version": "1.40.12"
    },
    "dataSources": {
      "mainService": {
        "uri": "srv-api/odata/v2/pg.MdCategoryService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      },
      "commonUtilService": {
        "uri": "srv-api/odata/v2/cm.util.CommonService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "sppCap_ui_dev"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "pg.md.mdVpItemMapping.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {},
        "sap.f": {},
        "sap.uxap": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "pg.md.mdVpItemMapping.i18n.i18n"
        }
      },
      "tblModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
      "util": {
        "dataSource": "commonUtilService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": true
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "groupId": "$direct",
          "synchronizationMode": "None",
          "autoExpandSelect": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "resourceRoots": {
      "ext.lib": "../../../../lib"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "pg.md.mdVpItemMapping.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
            "pattern": "",
            "name": "mdVpItemMapping",
            "target": [
                "mdVpItemMapping"
            ]
        }
      ],
      "targets": {
        "mdVpItemMapping": {
          "viewId": "mdVpItemMapping",
          "viewName": "mdVpItemMapping",
          "viewLevel": 1,
          "title": "{i18n>mainListViewTitle}"
        },
        "availableItems": {
          "viewName": "AvailableItems",
          "viewId": "availableItems"
        },
        "selectedItems": {
          "viewName": "SelectedItems",
          "viewId": "selectedItems"
        }
      }
    }
  }
}