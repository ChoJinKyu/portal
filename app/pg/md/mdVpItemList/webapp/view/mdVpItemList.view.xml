                        </t:template>
                    </t:Column>
                    <t:Column label="{I18N>/CREATE_USER_ID}"  hAlign="Center" width="8em">    
                        <t:template>
                            <Text text="{list>create_user_id}" wrapping="false"/>
                        </t:template>
                    </t:Column>
                    <t:Column label="{I18N>/UPDATE_USER_ID}" hAlign="Center" width="8em">    
                        <t:template>
                            <Text text="{list>update_user_id}" wrapping="false"/>
                        </t:template>
                    </t:Column>
                    <t:Column label="{I18N>/LOCAL_CREATE_DTM}" hAlign="Center" width="8em">    
                        <t:template>
                            <Text text="{path : 'list>local_create_dtm', formatter: '.dateFormatter.toUTCDateTimeString'}" wrapping="false"/>
                        </t:template>
                    </t:Column>
                    <t:Column label="{I18N>/LOCAL_CHANGE_DTM}" hAlign="Center"  width="8em">    
                        <t:template>
                            <Text text="{path : 'list>local_update_dtm', formatter: '.dateFormatter.toUTCDateTimeString'}" wrapping="false"/>
                        </t:template>
                    </t:Column>
                </t:columns>
            </t:TreeTable> 
        </VBox>
      </f:content>
    </f:DynamicPage>
</mvc:View>