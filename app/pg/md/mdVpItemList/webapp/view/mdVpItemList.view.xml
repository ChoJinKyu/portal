<mvc:View controllerName="pg.md.mdVpItemList.controller.mdVpItemList"
    class="sapUiSizeCompact"
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m" 
    xmlns:f="sap.f"  
    xmlns:t="sap.ui.table"
    xmlns:tnt="sap.tnt"
	xmlns:dnd="sap.ui.core.dnd"
    xmlns:form="sap.ui.layout.form" 
    xmlns:layout="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:lib-m="ext.lib.control.m"
    xmlns:lib-f="ext.lib.control.f"
    height="100%">


<f:DynamicPage id="dynamicPageId1" 
    headerExpanded="true" 
    fitContent="true"
    toggleHeaderOnTitleClick="true" >
      <!--DynamicPage Title -->
      <f:title>
            <lib-f:CustomDynamicPageTitle>
                <lib-f:heading>
                    <HBox alignItems="Center">
                        <!-- <Button icon="sap-icon://unfavorite" type="Transparent" /> -->
                        <Title text="{I18N>/MATERIAL_MANAGEMENT_CHAR_MNGT}"/>
                    </HBox>
                </lib-f:heading>
                <lib-f:fixedContent>
                    <form:SimpleForm class="searchBox"
                    maxContainerCols="2"
                    editable="true"
                    layout="ResponsiveGridLayout"
                    adjustLabelSpan="false"
                    labelSpanL="4"
                    labelSpanM="4"
                    emptySpanL="0"
                    emptySpanM="0"
                    columnsL="2"
                    columnsM="2">
                    <form:content>
                        
                    </form:content>
                    </form:SimpleForm>
                </lib-f:fixedContent>
            </lib-f:CustomDynamicPageTitle>
      </f:title>  
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <form:SimpleForm class="searchBox" maxContainerCols="2" layout="ResponsiveGridLayout" editable="true" 
                    adjustLabelSpan="false" 
                    labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                    <form:content>
                       <VBox>
                            <Label text="{I18N>/TENANT}" labelFor="searchTenantCombo" required="true"></Label>
                            <ComboBox id="searchTenantCombo" showSecondaryValues="true" width="100%" items="{
                                    path: 'org>/Org_Tenant',
                                    filters: []
                                }"
                                selectionChange="onChangeTenant">
                                <items>
                                    <core:ListItem key="{org>tenant_id}" text="{org>tenant_name}" additionalText="{org>tenant_id}" />
                                </items>
                            </ComboBox>
                            <layoutData>
                                <layout:GridData span="XL2 L3 M6 S12" />
                            </layoutData> 
                        </VBox>
                        <VBox>   
                            <Label text="{I18N>/BUSINESS_UNIT}" labelFor="searchChain" required="true"></Label>
                            <ComboBox id="searchChain" showSecondaryValues="true" width="100%" selectedKey="" >
                                <items>
                                    <core:ListItem key="{org>bizunit_code}" text="{org>bizunit_name}" additionalText="{org>bizunit_code}"/>
                                </items>
                            </ComboBox>
                            <layoutData>
                                <layout:GridData span="XL2 L3 M6 S12" />
                            </layoutData> 
                        </VBox>
                        <VBox>
                            <Label text= "{I18N>/VENDOR_POOL}" labelFor="search_Vp_Name"></Label>
                            <HBox>
                                <SearchField id="search_Vp_Name"
                                            search=".onDialogTreeCreate" 

                                            />
                                <Input id="search_Vp_Code" placeholder="Input data" visible ="false"></Input>
                            </HBox>
                            <layoutData>
                                <layout:GridData span="XL2 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label
                                text="{I18N>/STATUS}"
                                labelFor="search_codeGroupStatusflag">
                            </Label>
                            <SegmentedButton 
                                id="search_statusflag"
                                selectedKey="" 
                                width="100%">
                                <items>
                                    <SegmentedButtonItem text="{I18N>/ALL}" width="3em"/>
                                    <SegmentedButtonItem text="{I18N>/NEW}" width="3em" key="100"/>
                                    <SegmentedButtonItem text="{I18N>/TEMP_SAVE}" width="5em" key="200"/>
                                    <SegmentedButtonItem text="{I18N>/CONFIRMED}" width="3em" key="300"/>
                                </items>
                            </SegmentedButton>
                            <layoutData>
                                <layout:GridData
                                    span="XL2 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                            
                        <FlexBox alignItems="End" justifyContent="End" class="searchBoxButtonArea">
                            <Button id="pageSearchButton" text="{I18N>/SEARCH}" type="Emphasized" press=".onSearch" />
                            <layoutData>
                                <layout:GridData span="XL12 L12 M12 S12" />
                            </layoutData>
                        </FlexBox>
                    </form:content>
                </form:SimpleForm>
            </f:DynamicPageHeader>
        </f:header> 
         
      <f:content>
        <VBox>
                <!-- selectionMode="{= ${list>drill_state}==='leaf'? MultiToggle:None}" -->
            <t:TreeTable 
                id="treeTable"
                filter="treetablefilter"
                visibleRowCountMode="Auto"
                enableSelectAll="false"
                minAutoRowCount="16"
                fixedColumnCount="3"
                selectionBehavior="Row"
                selectionMode="None"
                onAfterRendering="onAfterRendering"
                enableColumnReordering="false"
                columnResize="onColumnResize"
                rows="{
                    path : 'list>/MdVpItemList',
                    parameters : {
                            arrayNames: ['nodes'],
                            numberOfExpandedLevels: 4
                    }
                }">
                <t:extension>
                    <OverflowToolbar>
                        <Title id="title" level="H2"/> 
                        <ToolbarSpacer/>
                        <MenuButton  text=" {I18N>/MANAGEMENT_CHARACTER_CONFIG} ">
                            <menu>
                                <Menu itemSelected="onMenuAction" id="searchCategory" items="{category>/MdCategory}">
                                    <items> 
                                        <MenuItem key="{category>spmd_category_code}" text="{category>spmd_category_code_name}"/>
                                    </items>
                                </Menu>
                            </menu>
                        </MenuButton>
                        <Button id="mainTableItemMappingButton" text="{I18N>/MANAGEMENT_CHARACTER_MAP}" type="Transparent" press=".onMainTableItemMappingButtonPress"  />
                        <Button id="mainTableConfirmButton" text="{I18N>/CONFIRMED}" type="Emphasized" press=".onMainTableConfirmButtonPress"/>
                        <ToolbarSeparator/>
                        <Button id="mainTableButtonAllClose" icon="sap-icon://collapse-all" type="Transparent" press=".onMainTablecollapseAll" />
                        <Button id="mainTableButtonAllOpen" icon="sap-icon://expand-all" type="Transparent" press=".onMainTableexpandAll" />
                        
                    </OverflowToolbar>
                </t:extension>
                
                <t:columns>
                    <!-- <t:Column label="" width="7rem">    
                        <t:template>
                            <CheckBox id="tableChkBox" selected="{list>checked}" fieldGroupIds="checkBoxs" select=".setIdSelect" visible="{= ${list>drill_state}==='leaf'? true:false}" />
                        </t:template>
                    </t:Column> -->
                    <t:Column label="{I18N>/VENDOR_POOL}" hAlign="Center" width="15rem">    
                        <t:template>
                            <HBox>
                                <CheckBox selected="{list>checked}" text="{list>vendor_pool_local_name}" select=".setIdSelect" visible="{= ${list>drill_state}==='leaf'? true:false}" />
                                <Text text="{list>vendor_pool_local_name}" wrapping="false" visible="{= ${list>drill_state}==='leaf'? false:true}"/>
