<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<form:SimpleForm id="midObjectForm1Edit" editable="false" >
		
		<!-- <core:Title text="Normal" /> -->	
        <Label text="{I18N>/SPMD_CHARACTER_CODE}"/>
        <Text text="{master>/spmd_character_code}"/>
        <Label text="{I18N>/SPMD_CHARACTER_CODE_NAME}" required="true"/>
        <Input value="{
                path: 'master>/spmd_character_code_name',
                type: 'sap.ui.model.type.String',
                constraints: {
                    maxLength: 50
                }
            }" required="true"></Input>       
        <Label text="{I18N>/SPMD_CHARACTER_DESC}"/>
        <Input value="{
                path: 'master>/spmd_character_desc',
                type: 'sap.ui.model.type.String',
                constraints: {
                    maxLength: 50
                }
            }" ></Input>	
	</form:SimpleForm>
</core:FragmentDefinition>