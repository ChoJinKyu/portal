<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core" 
    xmlns:l="sap.ui.layout" 
    xmlns:form="sap.ui.layout.form"
	xmlns:table="sap.ui.table" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog 
        id="dialog_history" 
        title="History" 
        resizable="true" 
        draggable="true" 
        contentWidth="55rem" 
        contentHeight="25rem"
        >
				<VBox id="vbox_part" class="sapUiResponsiveMargin">
					<!--<l:HorizontalLayout>-->
					<items>
							<Table id="historyTable"
                                items="{/MonitoringMasterHistoryManagement}" sticky="HeaderToolbar,ColumnHeaders">
                                	<headerToolbar>
                                    <OverflowToolbar>
                                        <content>
                                            <Title text="History List" level="H2"/>
                                            <!-- <ToolbarSpacer />
                                            <SearchField width="30%" class="sapUiSmallMargin" /> -->
                                        </content>
                                    </OverflowToolbar>
                                </headerToolbar>
                                <columns>
                                    <Column
                                        hAlign="Center"
                                        width="4rem">
                                        <Text text="회사" />
                                    </Column>
                                    <Column  
                                        hAlign="Center"
                                        width="5rem">
                                        <Text text="수정자" />
                                    </Column>
                                    <Column
                                        hAlign="Center"
                                        width="8rem">
                                        <Text text="수정항목" />
                                    </Column>
                                    <Column
                                        hAlign="Center"
                                        width="8rem">
                                        <Text text="수정시간" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem>
                                        <cells>
                                            <Text id="historytenant" />
                                            <Text id="historyModifier" />
                                            <Text text="" />
                                            <Text id="historyTime" />
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
					</items>
				</VBox>
        <endButton>
			<Button text="Cancle" press=".onPressDialogClose"/>
	    </endButton>
	</Dialog>
</core:FragmentDefinition>