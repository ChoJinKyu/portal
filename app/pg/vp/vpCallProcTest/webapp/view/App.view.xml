<mvc:View controllerName="pg.vp.vpCallProcTest.controller.App" 
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true" 
    xmlns="sap.m">	
    <Shell>
        <App id="app">
            <pages>
                <Page title="Proc Call Test">
                <content>
                    <Panel
                        headerText="Call Proc Test"
                        class="sapUiResponsiveMargin"
						width="auto">
                        <content>
                            <Button
                                text="Call VendorPool Mng"
                                icon="sap-icon://delete"
                                press=".onCallVendorPoolMng"
                                class="sapUiSmallMarginEnd"/>  
                            <Text
								text="Vendor Pool 관리 수정 Proc 실행합니다."
								class="sapUiSmallMargin"/>                               
                        </content>
                    </Panel>
                    <Panel
                        headerText="Call Return Obj Proc Test"
                        class="sapUiResponsiveMargin"
						width="auto">
                        <content>
                            <Button
                                text="Call VendorPool Mng"
                                icon="sap-icon://delete"
                                press=".onCallVendorPoolMngReturnObj"
                                class="sapUiSmallMarginEnd"/>  
                            <Text
								text="Vendor Pool 관리 수정 Proc 실행합니다.(Return Object)"
								class="sapUiSmallMargin"/>                               
                        </content>
                    </Panel>
                </content>
                </Page>
            </pages>
        </App>
	</Shell>
</mvc:View>
