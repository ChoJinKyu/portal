{
	"_version": "1.12.0",
	"sap.app": {
		"id": "pg.mi.miCategory",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		},
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/pg.marketIntelligenceService/",
                "type": "OData",
                "settings": {
                "odataVersion": "2.0"
                }
            }
        }
	},
	
	"sap.cloud": {
		"public": true,
		"service": "sppCap_ui_dev"
	},
	
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "pg.mi.miCategory.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pg.mi.miCategory.i18n.i18n"
				}
			},
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                "defaultBindingMode": "TwoWay",
                "defaultCountMode": "Inline",
                "refreshAfterChange": true,
                "useBatch": true
                }
            }
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
        },
        "resourceRoots": {
            "ext.lib": "../../../../lib"
        },
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "pg.mi.miCategory.view",
                "controlAggregation": "slide",
                "transition": "slide",
				"controlId": "flexibleColumnLayout"
			},
			"routes": [{
				"name": "Master",
				"pattern": "Master/{lflag},{category_code},{use_flag}",
                "target": ["TargetApp"],
                "targetControl": "Detail"
            },
            {
				"name": "Detail",
				"pattern": "Detail/{tenant_id},{company_code},{org_code},{org_type_code},{category_code},{category_name},{use_flag},{lflag},{p_category_code},{node_id},{p_node_id}",
                "target": ["TargetApp", "Detail"],
                "targetControl": "Detail"
            }

        
        
        
            ],
			"targets": {
				"TargetApp": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": true,
					"viewId": "Master",
					"viewName": "Master",
                    "viewLevel": 1,
					"controlAggregation": "beginColumnPages"
				},
				"Detail": {
					"viewType": "XML",
					"transition": "slide",
					"viewName": "Detail",
					"viewId": "Detail",
                    "viewLevel": 2,
					"controlAggregation": "midColumnPages"
                    
				}
			}
		}
	}
}