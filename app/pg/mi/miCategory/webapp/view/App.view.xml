<mvc:View
	displayBlock="true"
	height="100%"
    controllerName="pg.mi.miCategory.controller.App"
	xmlns="sap.f" xmlns:m="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
    <m:App id="app"> 
	<FlexibleColumnLayout id="flexibleColumnLayout" stateChange="onStateChanged" afterBeginColumnNavigate="onNavigateDaetail" backgroundDesign="Solid">
		<beginColumnPages>
			<mvc:XMLView id="beginView" viewName="pg.mi.miCategory.view.Master"/>
		</beginColumnPages>
		<midColumnPages>
			<mvc:XMLView id="detailView" viewName="pg.mi.miCategory.view.Detail"/>
		</midColumnPages>
	</FlexibleColumnLayout>
    </m:App>
</mvc:View>