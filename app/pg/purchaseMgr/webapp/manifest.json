{
	"_version": "1.12.0",
	"sap.app": {
		"id": "pg.purchaseMgr",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
        },
		"dataSources": {
			"view": {
				"uri": "purchaseMgr.json",
				"type": "JSON"
            },
            "odata": {
                "uri": "tempData.json",
                "type": "JSON"
            }
		}, 
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach",
		"sourceTemplate": {
			"id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
			"version": "1.40.12"
		}
	},
	
	"sap.cloud": {
		"public": true,
		"service": "sppCap_ui_dev"
	},
	
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		}
	},

	"sap.ui5": {
        "flexEnabled": false,
        "fullWidth": true,
		"rootView": {
			"viewName": "pg.purchaseMgr.view.Main",
			"type": "XML",
			"async": true,
			"id": "Main"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true,
                    "updateGroupId": "UpdateGroup",
                    "groupProperties": {
                        "MainUpdateGroup": {
                            "submit": "API"
                        },
                        "SubUpdateGroup": {
                            "submit": "API"
                        }
                    }
                }
            },    
			"view": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "view"
            },
			"odata": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "odata"
            },                    
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pg.purchaseMgr.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "pg.purchaseMgr.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteMain",
				"pattern": "RouteMain",
				"target": ["TargetMain"]
			}],
			"targets": {
				"TargetMain": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Main",
					"viewName": "Main"
				}
			}
		}
	}
}
