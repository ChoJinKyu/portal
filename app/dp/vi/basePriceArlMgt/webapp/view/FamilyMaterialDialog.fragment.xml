<core:FragmentDefinition
    class="sapUiSizeCompact"    
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:layout="sap.ui.layout"
    xmlns:table="sap.ui.table"
    xmlns:form="sap.ui.layout.form"
    displayBlock="true">
	
	<Dialog title="Family Material Code"
            contentWidth="90%"
            contentHeight="50%">
		<content>
            <table:Table   
                id="familyMaterialCodeTable"
                selectionMode="Single"
                rows="{dialogModel>/familyMaterialCode}"
                rowSelectionChange=".onSelectFamilyMaterialCode"
                fixedColumnCount="3">
                <table:extension>
                    <OverflowToolbar>
                        <Title text="Material Code" level="H2" />
                        <ToolbarSpacer />
                        <SearchField search="" width="15rem"/>
                    </OverflowToolbar>
                </table:extension>
                <table:columns>
                    <table:Column hAlign="Center" width="6rem">
                        <Label text="{I18N>/COMPANY}" textAlign="Center" />
                        <table:template>
                            <Text text="{detailModel>company_code}" />
                        </table:template>
                    </table:Column>
                    <table:Column hAlign="Center" width="6rem">
                        <Label text="{I18N>/BUSINESS_DIVISION}" textAlign="Center" />
                        <table:template>
                            <Text text="{detailModel>org_code}" />
                        </table:template>
                    </table:Column>
                    <table:Column hAlign="Center" width="10rem">
                        <Label text="{I18N>/MATERIAL_CODE}" />
                        <table:template>
                            <Text text="{detailModel>material_code}" />
                        </table:template>
                    </table:Column>
                    <table:Column headerSpan="2" hAlign="Center" width="6rem" visible="{= ${basePriceArlRootModel>/tenantId} === 'L2100' ? true : false}">
                        <table:multiLabels>
                            <Label text="{I18N>/SUPPLIER}" textAlign="Center" />
                            <Label text="{I18N>/CODE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="{detailModel>supplier_code}" />
                        </table:template>
                    </table:Column>
                    <table:Column width="10rem" hAlign="Center" visible="{= ${basePriceArlRootModel>/tenantId} === 'L2100' ? true : false}">
                        <table:multiLabels>
                            <Label text="{I18N>/SUPPLIER}" textAlign="Center" />
                            <Label text="{I18N>/Name}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="{detailModel>supplier_name}" />
                        </table:template>
                    </table:Column>
                    <table:Column hAlign="Center" width="10rem">
                        <Label text="{I18N>/CURRENT_ORDER_RATE}" />
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column headerSpan="9" hAlign="Center" width="10rem">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_PRICE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="{
                                path: 'detailModel>base_date',
                                formatter: '.dateFormatter.toDateString'}" />
                        </table:template>
                    </table:Column>
                    <table:Column width="10rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_CODE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/START_DATE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/BASE_PRICE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_CODE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/BASE_DATE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/NPD_BASE_PRICE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_CODE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/DOMESTIC}" textAlign="Center" />
                            <Label text="{I18N>/START_DATE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column headerSpan="9" hAlign="Center" width="10rem">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_PRICE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="{
                                path: 'detailModel>base_date',
                                formatter: '.dateFormatter.toDateString'}" />
                        </table:template>
                    </table:Column>
                    <table:Column width="10rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_CODE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/START_DATE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/BASE_PRICE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_CODE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/BASE_DATE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/NPD_BASE_PRICE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/CURRENCY_CODE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column width="6rem" hAlign="Center">
                        <table:multiLabels>
                            <Label text="{I18N>/EXPORT}" textAlign="Center" />
                            <Label text="{I18N>/START_DATE}" textAlign="Center" />
                        </table:multiLabels>
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column hAlign="Center" width="10rem">
                        <Label text="{I18N>/DESCRIPTION}" />
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                    <table:Column hAlign="Center" width="10rem">
                        <Label text="{I18N>/SPECIPICATION}" />
                        <table:template>
                            <Text text="" />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
		</content>
			<beginButton>
				<Button text="Close" press=".onClose">
                    <customData>
                        <core:CustomData key="fragmentName" value="_oFamilyMaterialDialog"  />
                    </customData>
                </Button>
			</beginButton>
            <endButton>
                <Button  text="Apply" 
                    type="Emphasized"
                    press=".onFamilyMaterialDetailApply" >
                    <customData>
                        <core:CustomData key="fragmentName" value="_oFamilyMaterialDialog"  />
                    </customData>
                </Button>
            </endButton>
	</Dialog>
</core:FragmentDefinition>