<mvc:View controllerName="dp.developmentReceipt.controller.developmentReceipt" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m" 
    xmlns:f="sap.f" 
    xmlns:form="sap.ui.layout.form" 
    xmlns:layout="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:table="sap.ui.table" 
    xmlns:sc="sap.suite.ui.commons" 
    xmlns:microchart="sap.suite.ui.microchart" 
    xmlns:lib-m="ext.lib.m"
    height="100%">

    <f:DynamicPage id="page"
        headerExpanded="false" 
        toggleHeaderOnTitleClick="true">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="{i18n>appTitle}" />
                </f:heading>
                <f:snappedContent>
                    <HBox>
                        <Text text=""/>
                    </HBox>
                    <HBox>
                        <Title text="Process"/>
                    </HBox>
                    <HBox>
                        <sc:MicroProcessFlow>
                            <sc:content>
                                <sc:MicroProcessFlowItem state="Error" stepWidth="3rem" icon="sap-icon://circle-task" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                            </sc:content>
                        </sc:MicroProcessFlow>
                    </HBox>
                    <HBox>
                        <Text text=" PR" renderWhitespace="true" />
                        <Text text="Budget" width="6em" textAlign="Right" />
                        <Text text="RFQ" width="6em" textAlign="Center" />
                        <Text text="   Mold&#10;   Spec" renderWhitespace="true" />
                        <Text text="Purchase&#10; Contract" width="8em" textAlign="Center" />
                        <Text text="PO" renderWhitespace="true" />
                        <Text text="RCV" width="5em" textAlign="Right" />
                        <Text text="Lease&#10;Contract" width="7.5em" textAlign="Center" />
                        <Text text="Invoice" />
                    </HBox>
                    <form:SimpleForm id="pageSearchFormS" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                        <form:content>
                            <VBox>
                                <Label text="Affiliate" labelFor="searchAffiliateS" required="true"></Label>
                                <MultiInput id="searchAffiliateS" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Division" labelFor="searchDivisionS" required="true"></Label>
                                <MultiInput id="searchDivisionS" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Receipt Date" labelFor="searchReceiptDateS" required="true"></Label>
                                <DateRangeSelection id="searchReceiptDateS" width="100%" change="receiptDateChange"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Status"></Label>
                                <SegmentedButton id="searchStatusS" selectedKey="" class="sapUiSmallMarginBottom" width="100%" selectionChange="onStatusSelectionChange" >
                                    <items>
                                        <SegmentedButtonItem text="All" key="all" />
                                        <SegmentedButtonItem text="Received" key="received" width="5em" />
                                        <SegmentedButtonItem text="Unreceived" key="unreceived" width="7em" />
                                    </items>
                                </SegmentedButton>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                        </form:content>
                    </form:SimpleForm>
                </f:snappedContent>
                <f:expandedContent>
                    <HBox>
                        <Text text=""/>
                    </HBox>
                    <HBox>
                        <Title text="Process"/>
                    </HBox>
                    <HBox>
                        <sc:MicroProcessFlow>
                            <sc:content>
                                <sc:MicroProcessFlowItem state="Error" stepWidth="3rem" icon="sap-icon://circle-task" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                            </sc:content>
                        </sc:MicroProcessFlow>
                    </HBox>
                    <HBox>
                        <Text text=" PR" renderWhitespace="true" />
                        <Text text="Budget" width="6em" textAlign="Right" />
                        <Text text="RFQ" width="6em" textAlign="Center" />
                        <Text text="   Mold&#10;   Spec" renderWhitespace="true" />
                        <Text text="Purchase&#10; Contract" width="8em" textAlign="Center" />
                        <Text text="PO" renderWhitespace="true" />
                        <Text text="RCV" width="5em" textAlign="Right" />
                        <Text text="Lease&#10;Contract" width="7.5em" textAlign="Center" />
                        <Text text="Invoice" />
                    </HBox>
                    <form:SimpleForm id="pageSearchFormE" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                        <form:content>
                            <VBox>
                                <Label text="Affiliate" labelFor="searchAffiliateE" required="true"></Label>
                                <MultiInput id="searchAffiliateE" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Division" labelFor="searchDivisionE" required="true"></Label>
                                <MultiInput id="searchDivisionE" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Receipt Date" labelFor="searchReceiptDateE" required="true"></Label>
                                    <DateRangeSelection id="searchReceiptDateE" width="100%" change="receiptDateChange"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Status"></Label>
                                <SegmentedButton id="searchStatusE" selectedKey="" class="sapUiSmallMarginBottom" width="100%" selectionChange="onStatusSelectionChange" >
                                    <items>
                                        <SegmentedButtonItem text="All" key="all" />
                                        <SegmentedButtonItem text="Received" key="received" width="5em" />
                                        <SegmentedButtonItem text="Unreceived" key="unreceived" width="7em" />
                                    </items>
                                </SegmentedButton>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                        </form:content>
                    </form:SimpleForm>
                </f:expandedContent>
                <f:actions>
                    <Button id="pageSearchButton" text="Search" type="Emphasized" press=".onPageSearchButtonPress" />
                </f:actions>
                <f:navigationActions>
                    <Button icon="sap-icon://unfavorite" type="Transparent" />
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <form:SimpleForm maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                    <form:content>
                        <VBox>
                            <Label text="Item Type " labelFor="searchItemType"></Label>
                            <ComboBox   id="searchItemType" width="100%"
                                        selectedKey=""
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ITEM_TYPE'}
                                                    ]
                                                }"
                            >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" linebreak="true" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Production Type" labelFor="searchProductionType"></Label>
                            <ComboBox   id="searchProductionType" width="100%"
                                        selectedKey=""
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_PROD_TYPE'}
                                                    ]
                                                }"
                            >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Description"></Label>
                            <Input id="searchDescription" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="E/D Type"></Label>
                            <ComboBox   id="searchEDType" width="100%"
                                        selectedKey=""
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ED_TYPE'}
                                                    ]
                                                }"
                            >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        
                        <VBox>
                            <Label text="Model" labelFor="searchModel"></Label>
                            <!--<MultiInput id="searchModel" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>-->
                            <Input id="searchModel" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Part No" labelFor="searchPartNo"></Label>
                            <!--<MultiInput id="searchPartNo" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>-->
                            <Input id="searchPartNo" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Family Part No" labelFor="searchFamilyPartNo"></Label>
                            <Input id="searchFamilyPartNo" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                    </form:content>
                </form:SimpleForm>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <table:Table id="moldMstTable"
                    selectionMode="MultiToggle"
                    fixedColumnCount="7"
                    visibleRowCountMode="Auto"
                    rows="{list>/MoldMasters}"
                    cellClick=".onCellClick"
                    rowSelectionChange=".onCheck">
                <table:extension>
                    <OverflowToolbar>
                        <Title text="List ({= ${list>/MoldMasters}.length || 0})" level="H2"/>
                        <ToolbarSpacer/>
                        <Button id="moldMstTableEditButton"     type="Transparent" text="Edit"               press=".onMoldMstTableEditButtonPress"     enabled="{mode>/viewFlag}" />
                        <ToolbarSeparator/>
                        <!-- Add the Personalization button -->
                        <Button id="moldMstTableBindButton"     type="Transparent" text="Bind &amp; Receipt" press=".onMoldMstTableBindButtonPress"     enabled="{mode>/viewFlag}" />
                        <Button id="moldMstTableCancelButton"   type="Transparent" text="Cancel Bind"        press=".onMoldMstTableCancelButtonPress"   enabled="{mode>/viewFlag}" />
                        <Button id="moldMstTableDeleteButton"   type="Transparent" text="Delete"             press=".onMoldMstTableDeleteButtonPress"   enabled="{mode>/viewFlag}" />
                        <Button id="moldMstTableReceiptButton"  type="Emphasized"  text="Receipt"            press=".onMoldMstTableReceiptButtonPress"  enabled="{mode>/viewFlag}" />
                        <!--
                        <ToolbarSeparator/>
                        <Button icon="sap-icon://action-settings" press=".onMoldMstTablePersoButtonPressed" >
                            <layoutData>
                                <OverflowToolbarLayoutData priority="NeverOverflow" />
                            </layoutData>
                        </Button>
                        -->
                    </OverflowToolbar>
                </table:extension>                             
                <table:columns>
                    <table:Column id="moldMstColumnAffiliate" width="5em" hAlign="Center">
                        <Label text="Affiliate"/>           
                        <table:template>
                            <Text id="company_code" text="{list>company_code}"/> 
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnDivision" width="4.5em" hAlign="Center">
                        <Label text="Division"/>
                        <table:template>
                            <Text id="org_code" text="{list>org_code}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnModel" width="15em" >
                        <Label text="Model"/>
                        <table:template>
                            <Text id="model" text="{list>model}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnPartNo" width="8em" hAlign="Center">
                        <Label text="Part No"/>
                        <table:template>
                            <Text id="part_number" text="{list>part_number}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnSeq" width="3em" hAlign="Center">
                        <Label text="Seq"/>
                        <table:template>
                            <Text id="mold_sequence" text="{list>mold_sequence}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnDescription" width="12em">
                        <Label text="Description"/>
                        <table:template>
                            <Text id="spec_name" text="{list>spec_name}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnStatus" width="8em" hAlign="Center">
                        <Label text="Status"/>
                        <table:template>
                            <!--<Text id="mold_receipt_flag" text="{list>mold_receipt_flag}"/>-->
                            <ComboBox selectedKey="{list>mold_receipt_flag}" editable="false" class="readonlyField">
                                <core:Item key="false" text="Unreceived"/>
                                <core:Item key="true" text="Received"/>
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnFamilyFlag" width="5.5em" hAlign="Center">
                        <VBox>
                        <Label text="Family"/>
                        <Label text="Y/N"/>
                        </VBox>
                        <table:template>
                            <ComboBox id="family_flag" selectedKey="{= ${list>family_part_number_1} !== null ? 'Y' : 'N' }" change="familyFlagChange" editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="Y" text="Yes"/>
                                <core:Item key="N" text="No"/>
                            </ComboBox>
                            <!--<ComboBox   id="family_flag"    
                                        selectedKey="{= ${list>family_part_number_1}.length > 0 ? 'Y' : 'N' }"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L2100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'CM_YN'}
                                                    ]
                                                }"
                            editable="{mode>/editFlag}" class="readonlyField">
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>-->
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnProductionType" width="12em" hAlign="Center">
                        <VBox>
                        <Label text="Production"/>
                        <Label text="Type" required="true"/>
                        </VBox>
                        <table:template>
                            <ComboBox   id="mold_production_type_code"
                                        selectedKey="{list>mold_production_type_code}"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_PROD_TYPE'}
                                                    ]
                                                }"
                            editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnItemType" width="10em" hAlign="Center">
                        <VBox>
                        <Label text="Item"/>
                        <Label text="Type" required="true"/>
                        </VBox>
                        <table:template>
                            <ComboBox   id="mold_item_type_code"
                                        selectedKey="{list>mold_item_type_code}"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ITEM_TYPE'}
                                                    ]
                                                }"
                            editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnMoldType" width="10em" hAlign="Center">
                        <VBox>
                        <Label text="Mold"/>
                        <Label text="Type" required="true"/>
                        </VBox>
                        <table:template>
                            <ComboBox   id="mold_type_code"
                                        selectedKey="{list>mold_type_code}"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_TYPE'}
                                                    ]
                                                }"
                            editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnEDType" width="10em" hAlign="Center">
                        <VBox>
                        <Label text="Export/Domestic"/>
                        <Label text="Type" required="true"/>
                        </VBox>
                        <table:template>
                            <ComboBox   id="export_domestic_type_code"
                                        selectedKey="{list>export_domestic_type_code}"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ED_TYPE'}
                                                    ]
                                                }"
                            editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnTestUseFlag" width="5em" hAlign="Center">
                        <VBox>
                        <Label text="T-EST"/>
                        <Label text="Use Y/N"/>
                        </VBox>
                        <table:template>
                            <Switch state="{list>t_est_use_flag}" customTextOn="Yes" customTextOff="No">
                                <layoutData>
                                    <FlexItemData growFactor="1" />
                                </layoutData>
                            </Switch>
                            <!--<CheckBox selected="{list>t_est_use_flag}" editable="{mode>/editFlag}" />-->
                            <!--<ComboBox   id="t_est_use_flag"    
                                        selectedKey="{list>t_est_use_flag}"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L2100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'CM_TF_FLAG'}
                                                    ]
                                                }"
                            >
                                <core:Item key="{= ${util>code} === 'true' ? true : false }" text="{util>code_description}" />
                            </ComboBox>-->
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnTestAttach" width="8em" hAlign="Center">
                        <VBox>
                        <Label text="T-EST"/>
                        <Label text="Attach"/>
                        </VBox>
                        <table:template>
                            <core:Icon 
                                src="sap-icon://attachment"
                                press="handleAttachmentPress" >
                                <core:layoutData>
                                    <FlexItemData growFactor="1" />
                                </core:layoutData>
                            </core:Icon>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnProcessType" width="8em" hAlign="Center">
                        <VBox>
                        <Label text="Process"/>
                        <Label text="Type"/>
                        </VBox>
                        <table:template>
                            <!--<Text id="import_mold_flag" text="{list>import_mold_flag}"/>-->
                            <ComboBox selectedKey="{list>import_mold_flag}" editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="false" text="Domestic"/>
                                <core:Item key="true" text="Export"/>
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnMoldStructure" width="10em" hAlign="Center">
                        <VBox>
                        <Label text="Mold"/>
                        <Label text="Structure" required="true"/>
                        </VBox>
                        <table:template>
                            <ComboBox   id="die_form"    
                                        selectedKey="{list>die_form}"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_STRUCTURE'}
                                                    ]
                                                }"
                            editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnSize" width="10em" hAlign="Center">
                        <Label text="Size" required="true"/>
                        <table:template>
                            <ComboBox   id="mold_size"    
                                        selectedKey="{list>mold_size}"
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_SIZE'}
                                                    ]
                                                }"
                            editable="{mode>/editFlag}" class="readonlyField" >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnReceiptDate" width="10em" hAlign="Center">
                        <VBox>
                        <Label text="Receipt"/>
                        <Label text="Date"/>
                        </VBox>
                        <table:template>
                            <Text id="receiving_report_date" text="{list>receiving_report_date}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnRequester" width="12em" hAlign="Center">
                        <Label text="Requester"/>
                            <table:template>
                                <Text id="develope_request_empno" text="{list>develope_request_empno}"/>
                            </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnMoldDeveloper" width="12em" hAlign="Center">
                        <VBox>
                        <Label text="Mold"/>
                        <Label text="Developer" required="true"/>
                        </VBox>
                        <table:template>
                            <Text id="mold_developer_id" text="{list>mold_developer_id}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnRemark" width="20em" hAlign="Center">
                        <Label text="Remark"/>
                        <table:template>
                            <TextArea id="remark" value="{list>remark}" rows="2" width="100%" editable="{mode>/editFlag}" class="readonlyField" />
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMstColumnProjectCode" width="15em">
                        <Label text="Project Code"/>
