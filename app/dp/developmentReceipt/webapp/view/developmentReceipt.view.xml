<mvc:View controllerName="dp.developmentReceipt.controller.developmentReceipt" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m" 
    xmlns:f="sap.f" 
    xmlns:form="sap.ui.layout.form" 
    xmlns:layout="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:table="sap.ui.table" 
    xmlns:sc="sap.suite.ui.commons" 
    xmlns:microchart="sap.suite.ui.microchart" 
    xmlns:lib-m="ext.lib.m"
    height="100%">

    <f:DynamicPage id="page"
        headerExpanded="false" 
        toggleHeaderOnTitleClick="true">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="{i18n>appTitle}" />
                </f:heading>
                <f:snappedContent>
                    <HBox>
                        <Text text=""/>
                    </HBox>
                    <HBox>
                        <Title text="Process"/>
                    </HBox>
                    <HBox>
                        <sc:MicroProcessFlow>
                            <sc:content>
                                <sc:MicroProcessFlowItem state="Error" stepWidth="3rem" icon="sap-icon://circle-task" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                            </sc:content>
                        </sc:MicroProcessFlow>
                    </HBox>
                    <HBox>
                        <Text text=" PR" renderWhitespace="true" />
                        <Text text="Budget" width="6em" textAlign="Right" />
                        <Text text="RFQ" width="6em" textAlign="Center" />
                        <Text text="   Mold&#10;   Spec" renderWhitespace="true" />
                        <Text text="Purchase&#10; Contract" width="8em" textAlign="Center" />
                        <Text text="PO" renderWhitespace="true" />
                        <Text text="RCV" width="5em" textAlign="Right" />
                        <Text text="Lease&#10;Contract" width="7.5em" textAlign="Center" />
                        <Text text="Invoice" />
                    </HBox>
                    <form:SimpleForm id="pageSearchFormS" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                        <form:content>
                            <VBox>
                                <Label text="Affiliate" labelFor="searchAffiliateS" required="true"></Label>
                                <MultiInput id="searchAffiliateS" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Division" labelFor="searchDivisionS" required="true"></Label>
                                <MultiInput id="searchDivisionS" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Receipt Date" labelFor="searchReceiptDateS" required="true"></Label>
                                <DateRangeSelection id="searchReceiptDateS" width="100%" change="handleChange"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Status"></Label>
                                <SegmentedButton selectedKey="" class="sapUiSmallMarginBottom" width="100%">
                                    <items>
                                        <SegmentedButtonItem text="All" key="" />
                                        <SegmentedButtonItem text="Received" width="5em" />
                                        <SegmentedButtonItem text="Unreceived" width="7em" />
                                    </items>
                                </SegmentedButton>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                        </form:content>
                    </form:SimpleForm>
                </f:snappedContent>
                <f:expandedContent>
                    <HBox>
                        <Text text=""/>
                    </HBox>
                    <HBox>
                        <Title text="Process"/>
                    </HBox>
                    <HBox>
                        <sc:MicroProcessFlow>
                            <sc:content>
                                <sc:MicroProcessFlowItem state="Error" stepWidth="3rem" icon="sap-icon://circle-task" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                                <sc:MicroProcessFlowItem state="None" stepWidth="3rem" />
                            </sc:content>
                        </sc:MicroProcessFlow>
                    </HBox>
                    <HBox>
                        <Text text=" PR" renderWhitespace="true" />
                        <Text text="Budget" width="6em" textAlign="Right" />
                        <Text text="RFQ" width="6em" textAlign="Center" />
                        <Text text="   Mold&#10;   Spec" renderWhitespace="true" />
                        <Text text="Purchase&#10; Contract" width="8em" textAlign="Center" />
                        <Text text="PO" renderWhitespace="true" />
                        <Text text="RCV" width="5em" textAlign="Right" />
                        <Text text="Lease&#10;Contract" width="7.5em" textAlign="Center" />
                        <Text text="Invoice" />
                    </HBox>
                    <form:SimpleForm id="pageSearchFormE" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                        <form:content>
                            <VBox>
                                <Label text="Affiliate" labelFor="searchAffiliateE" required="true"></Label>
                                <MultiInput id="searchAffiliateE" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Division" labelFor="searchDivisionE" required="true"></Label>
                                <MultiInput id="searchDivisionE" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Receipt Date" labelFor="searchReceiptDateE" required="true"></Label>
                                    <DateRangeSelection id="searchReceiptDateE" width="100%" change="handleChange"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="Status"></Label>
                                <SegmentedButton selectedKey="all" class="sapUiSmallMarginBottom" width="100%">
                                    <items>
                                        <SegmentedButtonItem text="All" key="all" />
                                        <SegmentedButtonItem text="Received" width="5em" />
                                        <SegmentedButtonItem text="Unreceived" width="7em" />
                                    </items>
                                </SegmentedButton>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                        </form:content>
                    </form:SimpleForm>
                </f:expandedContent>
                <f:actions>
                    <Button id="pageSearchButton" text="Search" type="Emphasized" press=".onPageSearchButtonPress" />
                </f:actions>
                <f:navigationActions>
                    <Button icon="sap-icon://unfavorite" type="Transparent" />
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <form:SimpleForm maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                    <form:content>
                        <VBox>
                            <Label text="Item Type " labelFor="searchItemType"></Label>
                            <MultiInput id="searchItemType" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" linebreak="true" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Production Type" labelFor="searchProductionType"></Label>
                            <MultiInput id="searchProductionType" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Description"></Label>
                            <Input id="searchDescription" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="E/D Type"></Label>
                            <ComboBox   id="searchEDType" width="80%"
                                        selectedKey=""
                                        items="{
                                                    path : 'util>/CodeDetails',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                        {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ED_TYPE'}
                                                    ]
                                                }"
                            >
                                <core:Item key="{util>code}" text="{util>code_description}" />
                            </ComboBox>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        
                        <VBox>
                            <Label text="Model " labelFor="searchModel"></Label>
                            <MultiInput id="searchModel" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Part No " labelFor="searchPartNo"></Label>
                            <MultiInput id="searchPartNo" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="Family Part No" labelFor="searchFamilyPartNo"></Label>
                            <Input id="searchFamilyPartNo" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                    </form:content>
                </form:SimpleForm>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <VBox>
                <table:Table id="moldMstTable"
                        selectionMode="MultiToggle"
                        fixedColumnCount="7"
                        class="sapFDynamicPageAlignContent" 
                        cellClick=".onCellClick"
                        rowSelectionChange = ".onCheck"  
                        updateFinished=".onMoldMstTableUpdateFinished"
                        rows="{ path : '/MoldMasters',
                                parameters : {
                                    $count : true,
                                    $$updateGroupId : 'moldMstUpdateGroup'
                                },
                                filters : [
                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                        {path : 'company_code', operator : 'EQ', value1 : 'LGEKR'}
                                    ],
                                    sorter: [
                                        { path: 'tenant_id' }, 
                                        { path: 'company_code' },
                                        { path: 'org_code' }
                                    ]
                        }"               
                        >
                    <table:extension>
                        <OverflowToolbar>
                            <ToolbarSpacer/>
                            <!-- Add the Personalization button -->
                            <Button id="moldMstTableBindButton" text="Bind &amp; Receipt" type="Transparent" press=".onMoldMstTableBindButtonPress" />
                            <Button id="moldMstTableCancelButton" text="Cancel Bind" type="Transparent" press=".onMoldMstTableCancelButtonPress" />
                            <Button id="moldMstTableDeleteButton" text="Delete" type="Transparent" press=".onMoldMstTableDeleteButtonPress" />
                            <Button id="moldMstTableReceiptButton" text="Receipt" type="Emphasized" press=".onMoldMstTableReceiptButtonPress" />
                            <ToolbarSeparator/>
                            <Button icon="sap-icon://action-settings" press=".onMoldMstTablePersoButtonPressed" >
                                <layoutData>
                                    <OverflowToolbarLayoutData priority="NeverOverflow" />
                                </layoutData>
                            </Button>
                        </OverflowToolbar>
                    </table:extension>                             
                    <table:columns>
                        <table:Column id="moldMstColumnAffiliate" width="6em" hAlign="Center">
                            <Label text="Affiliate"/>           
                            <table:template>
                                <Text id="company_code" text="{company_code}"/> 
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnDivision" width="6em" hAlign="Center">
                            <Label text="Division"/>
                            <table:template>
                                <Text id="org_code" text="{org_code}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnModel" width="15em" >
                            <Label text="Model"/>
                            <table:template>
                                <Text id="model" text="{model}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnPartNo" width="8em" hAlign="Center">
                            <Label text="Part No"/>
                            <table:template>
                                <Text id="part_number" text="{part_number}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnSeq" width="5em" hAlign="Center">
                            <Label text="Seq"/>
                            <table:template>
                                <Text id="mold_sequence" text="{mold_sequence}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnDescription" width="12em">
                            <Label text="Description"/>
                            <table:template>
                                <Text id="spec_name" text="{spec_name}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnStatus" width="7em" hAlign="Center">
                            <Label text="Status"/>
                            <table:template>
                                <Text id="mold_receipt_flag" text="{mold_receipt_flag}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnFamilyFlag" width="8em" hAlign="Center">
                            <Label text="Family Y/N"/>
                            <table:template>
                                <ComboBox   id="family_flag"    
                                            selectedKey="{family_flag}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'CM_YN'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnProductionType" width="12em" hAlign="Center">
                            <Label text="Production&#10;Type"/>
                            <table:template>
                                <ComboBox   id="mold_production_type_code"
                                            selectedKey="{mold_production_type_code}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_PROD_TYPE'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnItemType" width="10em" hAlign="Center">
                            <Label text="Item&#10;Type"/>
                            <table:template>
                                <ComboBox   id="mold_item_type_code"
                                            selectedKey="{mold_item_type_code}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ITEM_TYPE'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnMoldType" width="10em" hAlign="Center">
                            <Label text="Mold&#10;Type"/>
                            <table:template>
                                <ComboBox   id="mold_type_code"
                                            selectedKey="{mold_type_code}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_TYPE'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnEDType" width="10em" hAlign="Center">
                            <Label text="Export/Domestic&#10;Type"/>
                            <table:template>
                                <ComboBox   id="export_domestic_type_code"
                                            selectedKey="{export_domestic_type_code}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ED_TYPE'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnTestUseFlag" width="8em" hAlign="Center">
                            <Label text="T-EST&#10;Use Y/N"/>
                            <table:template>
                                <ComboBox   id="t_est_use_flag"    
                                            selectedKey="{t_est_use_flag}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : 'G100'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'CM_TF_FLAG'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnTestAttach" width="8em" hAlign="Center">
                            <Label text="T-EST&#10;Attach"/>
                            <table:template>
                                <core:Icon 
                                    src="sap-icon://attachment"
                                    press="handleAttachmentPress" >
                                    <core:layoutData>
                                        <FlexItemData growFactor="1" />
                                    </core:layoutData>
                                </core:Icon>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnProcessType" width="8em" hAlign="Center">
                            <Label text="Process&#10;Type"/>
                            <table:template>
                                <Text id="import_mold_flag" text="{import_mold_flag}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnMoldStructure" width="10em" hAlign="Center">
                            <Label text="Mold&#10;Structure"/>
                            <table:template>
                                <ComboBox   id="die_form"    
                                            selectedKey="{die_form}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_STRUCTURE'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnSize" width="10em" hAlign="Center">
                            <Label text="Size"/>
                            <table:template>
                                <ComboBox   id="mold_size"    
                                            selectedKey="{mold_size}"
                                            items="{
                                                        path : 'util>/CodeDetails',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L1100'},
                                                            {path : 'company_code', operator : 'EQ', value1 : '*'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_SIZE'}
                                                        ]
                                                    }"
                                >
                                    <core:Item key="{util>code}" text="{util>code_description}" />
                                </ComboBox>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnReceiptDate" width="10em" hAlign="Center">
                            <Label text="Receipt&#10;Date"/>
                            <table:template>
                                <Text id="receiving_report_date" text="{receiving_report_date}"/>
                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnRequester" width="12em" hAlign="Center">
                            <Label text="Requester"/>
                                <table:template>
	                                <Text id="develope_request_empno" text="{develope_request_empno}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnMoldDeveloper" width="12em" hAlign="Center">
                            <Label text="Mold&#10;Developer"/>
                                <table:template>
	                                <Text id="mold_developer_id" text="{mold_developer_id}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnRemark" width="20em" hAlign="Center">
                            <Label text="Remark"/>
                                <table:template>
	                                <Input id="remark" value="{remark}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnProjectCode" width="15em">
                            <Label text="Project Code"/>
                                <table:template>
	                                <Text id="pdm_project_code" text="{pdm_project_code}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnProjectName" width="20em">
                            <Label text="Project Name"/>
                                <table:template>
	                                <Text id="pdm_project_name" text="{pdm_project_name}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnFamilyPartNo1" width="12em" hAlign="Center">
                            <Label text="Family&#10;Part No(1)"/>
                                <table:template>
	                                <Input id="family_part_number_1" value="{family_part_number_1}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnFamilyPartNo2" width="12em" hAlign="Center">
                            <Label text="Family&#10;Part No(2)"/>
                                <table:template>
	                                <Input id="family_part_number_2" value="{family_part_number_2}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnFamilyPartNo3" width="12em" hAlign="Center">
                            <Label text="Family&#10;Part No(3)"/>
                                <table:template>
	                                <Input id="family_part_number_3" value="{family_part_number_3}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnFamilyPartNo4" width="12em" hAlign="Center">
                            <Label text="Family&#10;Part No(4)"/>
                                <table:template>
	                                <Input id="family_part_number_4" value="{family_part_number_4}"/>
	                            </table:template>
                        </table:Column>
                        <table:Column id="moldMstColumnFamilyPartNo5" width="12em" hAlign="Center">
                            <Label text="Family&#10;Part No(5)"/>
                                <table:template>
	                                <Input id="family_part_number_5" value="{family_part_number_5}"/>
	                            </table:template>
                        </table:Column>
                    </table:columns>
                </table:Table>
            </VBox>
        </f:content>
    </f:DynamicPage>
</mvc:View>