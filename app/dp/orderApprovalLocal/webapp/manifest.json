{
    "_version": "1.12.0",
    "sap.app": {
        "id": "dp.orderApprovalLocal",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "ach": "set-ach",
        "resources": "resources.json",
        "dataSources": {
            "mainService": {
                "uri": "srv-api/odata/v2/dp.OrderApprovalService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "moldService": {
                "uri": "srv-api/odata/v2/dp.DetailSpecEntryService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "commonUtilService": {
                "uri": "srv-api/odata/v2/cm.util.CommonService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "hrUtilService": {
                "uri": "srv-api/odata/v2/cm.util.HrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "orgMgrService": {
                "uri": "srv-api/odata/v2/cm.OrgMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "purOrgMgrService": {
                "uri": "srv-api/odata/v2/cm.PurOrgMgrService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://task",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "dp.orderApprovalLocal.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.66.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "dp.orderApprovalLocal.i18n.i18n"
                }
            },
            "util": {
                "dataSource": "commonUtilService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": true
                }
            },
            "hr": {
                "dataSource": "hrUtilService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": true
                }
            },
            "org": {
                "dataSource": "orgMgrService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true
                }
            },
            "purOrg": {
                "dataSource": "purOrgMgrService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": true
                }
            },
            "moldMng": {
                "dataSource": "moldService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": true
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "resourceRoots": {
            "ext.lib": "../../../lib",
            "dp.util": "../../util"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "dp.orderApprovalLocal.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "bypassed": {
                    "target": [
                        "notFound"
                    ]
                },
                "async": true
            },
            "routes": [
                {
                    "pattern": "poaCreateObject/{company_code}/{plant_code}",
                    "name": "poaCreateObject",
                    "target": [
                        "poaCreateObject"
                    ]
                },
                {
                    "pattern": "poaEditObject/{company_code}/{plant_code}/{approval_number}",
                    "name": "poaEditObject",
                    "target": [
                        "poaEditObject"
                    ]
                },
                {
                    "pattern": "poaObject/{approval_number}",
                    "name": "poaObject",
                    "target": [
                        "poaObject"
                    ]
                }
            ],
            "targets": {
                "poaCreateObject": {
                    "viewName": "PoaCreateObject",
                    "viewId": "poaCreateObject",
                    "viewLevel": 1,
                    "title": "{i18n>poaCreateObjectTitle}"
                },
                "poaEditObject": {
                    "viewName": "PoaCreateObject",
                    "viewId": "poaCreateObject",
                    "viewLevel": 1,
                    "title": "{i18n>poaCreateObjectTitle}"
                },
                "poaObject": {
                    "viewName": "PoaObject",
                    "viewId": "poaObject",
                    "viewLevel": 1,
                    "title": "{i18n>poaObjectTitle}"
                },
                "notFound": {
                    "viewName": "NotFound",
                    "viewPath": "ext.lib.view",
                    "viewId": "notFound"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sppCap_ui_dev"
    }
}