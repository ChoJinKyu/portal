{
  "_version": "1.12.0",
  
  "sap.app": {
    "id": "dp.im.supplierIdeaMgt",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "srv-api/odata/v2/dp.SupplierIdeaMgtService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "commonUtilService": {
        "uri": "srv-api/odata/v2/cm.util.CommonService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
        "v4Proc": {
            "uri": "srv-api/odata/v4/dp.SupplierIdeaMgtV4Service/",
            "type": "OData",
            "settings": {
                "odataVersion": "4.0"
            }
        }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "sppCap_ui_dev"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "dp.im.supplierIdeaMgt.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.60.1",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "dp.im.supplierIdeaMgt.i18n.i18n"
        }
      },
      "util": {
        "dataSource": "commonUtilService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "TwoWay",
          "defaultCountMode": "Inline",
          "refreshAfterChange": false,
          "useBatch": true
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "groupId": "$direct",
          "synchronizationMode": "None",
          "autoExpandSelect": true
        }
      },
        "v4Proc": {
            "dataSource": "v4Proc",
            "preload": false,
            "settings": {
                "synchronizationMode": "None",
                "operationMode": "Server",
                "autoExpandSelect": false
            }
        }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "resourceRoots": {
      "ext.lib": "../../../../lib"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "dp.im.supplierIdeaMgt.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "pattern": "",
          "name": "supplierIdeaMgt",
          "target": [
            "supplierIdeaMgt"
          ]
        },
        {
            "pattern": "supplierIdeaDetail/{tenantId}/{fundingNotifyNumber}",
            "name": "supplierIdeaDetail",
            "target": [
                "supplierIdeaDetail"
            ]
        }
      ],
      "targets": {
        "supplierIdeaMgt": {
          "viewName": "SupplierIdeaMgt",
          "viewId": "supplierIdeaMgt",
          "viewLevel": 1,
          "title": "{{appTitle}}"
        },
        "supplierIdeaDetail": {
            "viewName": "SupplierIdeaDetail",
            "viewId": "supplierIdeaDetail",
            "viewLevel": 2
        },
        "notFound": {
            "viewName": "NotFound",
            "viewPath": "ext.lib.view",
            "viewId": "notFound"
        }
      }
    }
  }
}