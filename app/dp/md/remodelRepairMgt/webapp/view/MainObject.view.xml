<mvc:View controllerName="dp.md.remodelRepairMgt.controller.MainObject" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:uxap="sap.uxap" 
	xmlns:layout="sap.ui.layout" 
    xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
    core:require="{ I18NFormatter: 'dp/md/util/controller/I18NFormatter' }"
	height="100%">

	<uxap:ObjectPageLayout id="page"
		toggleHeaderOnTitleClick="true"
		alwaysShowContentHeader = "false"
		preserveHeaderStateOnScroll="false"
		headerContentPinnable="true"
		isChildPage="true"
		upperCaseAnchorBar="false"
		showTitleInHeaderContent="false"
		showEditHeaderButton="true" 
		showFooter="true">
		<uxap:headerTitle>
			<uxap:ObjectPageDynamicHeaderTitle>
                <uxap:heading>
                    <HBox alignItems="Center">
                        <OverflowToolbarButton type="Transparent" icon="sap-icon://nav-back" press=".onPageNavBackButtonPress"  tooltip="Close column" visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"/>
                        <Title text="Remodel &amp; Repair Detail" wrapping="true" class="" />
                    </HBox>
                </uxap:heading>
                <uxap:navigationActions>
					
                </uxap:navigationActions>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>

		<uxap:sections id="testFrag">
			<uxap:ObjectPageSection id="pageSectionMain" titleUppercase="false" title="Request Entry">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
                        <VBox class="sapUiSmallMargin">
                            <form:SimpleForm editable="false" layout="ResponsiveGridLayout" >
                                <form:content>
                                    <VBox height="3.5rem">
                                        <Label text="Submission Title"/>
                                        <Input value="{repairItem>/repair_desc}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Submission No"/> 
                                        <Text text="{repairItem>repair_request_number}"/> 
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Factor"/> 
                                        <Select width="100%"
                                                forceSelection="false"
                                                selectedKey="{repairItem>/repair_factor}"
                                                items="{
                                                            path : 'util>/Code',
                                                            filters : [
                                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_REPAIR_FACTOR'}
                                                            ]
                                                        }"
                                                editable="{mode>/editFlag}" >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Suppliers Remarks"/>
                                        <Input value="{repairItem>/repair_reason}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Requester"/>
                                        <Text text="{repairItem>/create_user_id}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Request Date"/>
                                        <Text text="{repairItem>/repair_request_date}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="LGE Remarks"/>
                                        <Input value="{repairItem>/remark}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Remodel/Repair"/>
                                        <Select width="100%"
                                                forceSelection="false"
                                                selectedKey="{repairItem>/repair_type_code}"
                                                items="{
                                                            path : 'util>/Code',
                                                            filters : [
                                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_REPAIR_TYPE'}
                                                            ]
                                                        }"
                                                editable="{mode>/editFlag}"
                                                 >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Domestic/Export"/>
                                        <Select width="100%"
                                                forceSelection="false"
                                                selectedKey="{repairItem>/mold_location_type_code}"
                                                items="{
                                                            path : 'util>/Code',
                                                            filters : [
                                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ED_TYPE'}
                                                            ]
                                                        }"
                                                editable="{mode>/editFlag}"
                                                 >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Account"/>
                                        <Select width="100%"
                                                forceSelection="false"
                                                selectedKey="{repairItem>/account_code}"
                                                items="{
                                                            path : 'util>/Code',
                                                            filters : [
                                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ACCOUNT_CODE'}
                                                            ]
                                                        }"
                                                editable="{mode>/editFlag}" >
                                            <core:Item key="{util>code}" text="[{util>code}]{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Investment/Cost"/>
                                        <Select width="100%"
                                                forceSelection="false"
                                                selectedKey="{repairItem>/investment_ecst_type_code}"
                                                items="{
                                                            path : 'util>/Code',
                                                            filters : [
                                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_INVESTMENT_EXPENSE_TYPE'}
                                                            ]
                                                        }"
                                                editable="false" >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Spare Part"/>
                                        <Select width="100%"
                                                forceSelection="false"
                                                selectedKey="{repairItem>/investment_ecst_type_code}"
                                                items="{
                                                            path : 'util>/Code',
                                                            filters : [
                                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                                {path : 'group_code', operator : 'EQ', value1 : 'CM_YN'}
                                                            ]
                                                        }"
                                                editable="{mode>/editFlag}"
                                                 >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL2 L2 M2 S3" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Spare Part Seq"/>
                                        <Text text="0"/>
                                        <layoutData>
                                            <layout:GridData span="XL2 L2 M2 S3" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="ECO Creator"/>
                                        <Text text="{repairItem>eco_number}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Activity"/>
                                        <Select   width="100%"
                                                    forceSelection="false"
                                                    selectedKey="{repairItem>/activity_code}"
                                                    items="{
                                                                path : 'util>/Code',
                                                                filters : [
                                                                    {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                                    {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_SIZE'}
                                                                ]
                                                            }"
                                                    editable="{mode>/editFlag}"
                                                    
                                        >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                </form:content>
                            </form:SimpleForm>
                        </VBox>
                    </uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

			<uxap:ObjectPageSection titleUppercase="false" title="Remodel/Repair Info">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
                        <VBox class="sapUiSmallMargin">
                            <form:SimpleForm editable="false" layout="ResponsiveGridLayout" >
                                <form:content>
                                    <VBox height="3.5rem">
                                        <Label text="Model"/>
                                        <Text text="{moldView>model}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Mold No"/> 
                                        <Text text="{moldView>mold_number}"/>
                                        <layoutData>
                                            <layout:GridData span="XL2 L2 M2 S3" />
                                        </layoutData>
                                     </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Seq"/> 
                                        <Text text="{moldView>mold_sequence}"/>
                                        <layoutData>
                                            <layout:GridData span="XL2 L2 M2 S3" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Currency"/> 
                                        <Text text="{repairItem>currency_code}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Item Type"/>
                                        <Select selectedKey="{moldView>mold_item_type_code}"
                                            items="{
                                                        path : 'util>/Code',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ITEM_TYPE'}
                                                        ]
                                                    }"
                                            editable="false" >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Item Name"/>
                                        <Text text="{moldView>class_desc}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Die Machine"/>
                                        <Text text="{moldView>mold_tonnage}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Material"/>
                                        <Text text="{moldView>use_material_value}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Specification Number"/>
                                        <Input value="{repairItem>eco_number}" placeholder="Search" required="true"
                                            editable="{mode>/editFlag}"
                                            showValueHelp="true" valueHelpRequest=".onInputWithEmployeeValuePress">
                                        </Input>
                                        <layoutData>
                                            <layout:GridData span="XL2 L2 M2 S3" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text=""/>
                                        <Text text="{repairItem>/eco_number}"/>
                                        <layoutData>
                                            <layout:GridData span="XL2 L2 M2 S3" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Mold Supplier"/>
                                        <Text text="{moldView>/supplier_name}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Prod Supplier"/>
                                        <Text text="{moldView>/production_supplier_name}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Etc1"/>
                                        <Text text="{moldView>/asset_number}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Etc2"/>
                                        <Text text="{moldView>/asset_number}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Cavity/Process Qty"/>
                                        <Text text="{moldView>/cavity_process_qty}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Assets Type"/>
                                        <Select selectedKey="{moldView>/asset_type_code}"
                                            items="{
                                                        path : 'util>/Code',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ASSET_TYPE'}
                                                        ]
                                                    }"
                                            editable="false" >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Assets Status"/>
                                        <Select selectedKey="{moldView>/asset_status_code}"
                                            items="{
                                                        path : 'util>/Code',
                                                        filters : [
                                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ASSET_STATUS'}
                                                        ]
                                                    }"
                                            editable="false" >
                                            <core:Item key="{util>code}" text="{util>code_name}" />
                                        </Select>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Receipt Date"/>
                                        <Text text="{path: 'moldView>/acq_date', formatter: '.dateFormatter.toDateString'}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Sample Qty"/>
                                        <Input value="{repairItem>/sample_quantity}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Size"/>
                                        <Input value="{repairItem>/repair_mold_size}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Account Dept"/>
                                        <Input value="{repairItem>accounting_department_code}" placeholder="Search" required="true"
                                            editable="{mode>/editFlag}"
                                            showValueHelp="true" valueHelpRequest=".onInputWithEmployeeValuePress">
                                        </Input>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Account Dept Code"/>
                                        <Input value="{repairItem>/accounting_department_code}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Event"/>
                                        <Input value="{repairItem>/develope_step_code}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="RepairÂ Supplier"/>
                                        <Input value="{repairItem>repair_supplier_name}" placeholder="Search" required="true"
                                            editable="{mode>/editFlag}"
                                            showValueHelp="true" valueHelpRequest=".onInputWithEmployeeValuePress">
                                        </Input>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Move Plan"/>
                                        <DatePicker value="{repairItem>mold_moving_plan_date}" valueFormat="yyyyMMdd" displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Complete Plan"/>
                                        <DatePicker value="{repairItem>mold_complete_plan_date}" valueFormat="yyyyMMdd" displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Repair Supplier Code"/>
                                        <Text text="{repairItem>repair_supplier_code}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Move Result"/>
                                        <DatePicker value="{repairItem>mold_moving_result_date}" valueFormat="yyyyMMdd" displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Complete Result"/>
                                        <DatePicker value="{repairItem>mold_complete_result_date}" valueFormat="yyyyMMdd" displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Project Code"/>
                                        <Input value="{repairItem>/project_code}" editable="{mode>/editFlag}" />
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Acquisition Dept"/>
                                        <Input value="{repairItem>acq_department_code}" placeholder="Search" required="true"
                                            editable="{mode>/editFlag}" width="70%"
                                            showValueHelp="true" valueHelpRequest=".onInputWithEmployeeValuePress">
                                        </Input>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Balanced Budget"/>
                                        <Text text="{repairItem>/budget_amount}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                    <VBox height="3.5rem">
                                        <Label text="Family Part No"/>
                                        <Text text="{moldView>/family_part_numbers}"/>
                                        <layoutData>
                                            <layout:GridData span="XL4 L4 M4 S6" />
                                        </layoutData>
                                    </VBox>

                                </form:content>
                            </form:SimpleForm>
                        </VBox>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

            <uxap:ObjectPageSection titleUppercase="false" title="Remodel/Repair History" visible="{= !${mode>/newFlag}}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
                        <Table id="historyTable"
                                mode="None"
                                sticky="HeaderToolbar,ColumnHeaders"
                                inset="false"
                                class="sapFDynamicPageAlignContent"
                                width="auto"
                                items= "{schedule>/MoldSchedule}" >
                            <columns>
                                <Column>
                                    <Label text="Type" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Req Date" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Remodel/Repair Detail" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Order Sheet Number" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Remodel/Repair Supp" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Receipt Date" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Specification Number" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Currency" wrapping="true" />
                                </Column>
                                <Column>
                                    <Label text="Amount" wrapping="true" />
                                </Column>
                            </columns>
                            <items>
                                <ColumnListItem>
                                    <cells>
                                        <Text text="{history>/repair_type_name}"/>
                                        <Text text="{path: 'history>repair_request_date', formatter: '.dateFormatter.toDateString'}"/>
                                        <Text text="{history>/repair_desc}"/>
                                        <Text text="{history>/repair_request_number}"/>
                                        <Text text="{history>/repair_supplier_name}"/>
                                        <Text text="{path: 'history>complete_date', formatter: '.dateFormatter.toDateString'}"/>
                                        <Text text="{history>/eco_number}"/>
                                        <Text text="{history>/currency_code}"/>
                                        <Text text="{path: 'history>repair_amount', formatter: '.numberFormatter.toNumberString'}"/>
                                    </cells>
                                </ColumnListItem>
                            </items>
                        </Table>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

		</uxap:sections>
		
		<uxap:footer>
			<OverflowToolbar>
				<ToolbarSpacer />
                <Button id="pageEditButton"         type="Transparent" text="{I18N>/EDIT}"   press=".onPageEditButtonPress" visible="{= ${mode>/newFlag} ? false : !${mode>/editFlag}}" />
				<Button id="pageCancelButton"       type="Transparent" text="{I18N>/CANCEL}" press=".onPageCancelEditButtonPress" visible="{= ${mode>/newFlag} ? false : ${mode>/editFlag}}" />
                <!--<Button id="pageDraftButton"        type="Transparent" text="{I18N>/DRAFT}"  press=".onPageSaveButtonPress" visible="{= !(${moldView>/mold_spec_status_code} === 'C') }" />-->
                <Button id="pagePartNoSearchButton" type="Transparent" text="Part No Search"  press=".onPagePartNoSearchButtonPress"  /><!--visible="{= !(${moldView>/mold_spec_status_code} === 'C') }"-->
				<Button id="pageConfirmButton"      type="Emphasized"  text="Confirm" press=".onPageConfirmButtonPress" />
			</OverflowToolbar>
		</uxap:footer>
	</uxap:ObjectPageLayout>
</mvc:View>