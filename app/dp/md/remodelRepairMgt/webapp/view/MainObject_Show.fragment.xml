<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
    xmlns:uxap="sap.uxap" >
    <VBox class="sapUiSmallMargin">
        <form:SimpleForm 
            editable="false"
            layout="ResponsiveGridLayout"
            >
            <form:content>
                <VBox height="3.5rem">
                    <Label text="Submission Title"/>
                    <Input value="{spec>/use_material_value}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Submission No"/> 
                    <Text text="{spec>company_code}"/> 
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Factor"/> 
                    <Select width="100%"
                            forceSelection="false"
                            selectedKey="{master>/mold_type_code}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_TYPE'}
                                        ]
                                    }"
                            editable="false"
                            class="readonlyField" >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Vendor Remarks"/>
                    <Input value="{spec>/order_qty}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Requester"/>
                    <Text text="{spec>/cavity_process_qty}"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Request Date"/>
                    <Text text="{spec>/cavity_process_qty}"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="LGE Remarks"/>
                    <Input value="{spec>/mold_sample_qty}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Remodel/Repair"/>
                    <Input value="{spec>/request_life_shot_count}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Domestic/Export"/>
                    <Input value="{spec>/mold_weight}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Account"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{master>/mold_type_code}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_TYPE'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Investment/Cost"/>
                    <Input value="{spec>/mold_hot_runner_qty}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Spare Part"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/hot_runner_maker_name}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_HR_MAKER'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="ECO Creator"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/die_form}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_STRUCTURE'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Activity"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/mold_size}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_SIZE'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

            </form:content>
        </form:SimpleForm>



            <form:SimpleForm 
                editable="false"
                layout="ResponsiveGridLayout"
                >

                <form:content>

                <core:Title text="Spare Parts" />

                <VBox height="3.5rem">
                    <Label text="Eye-Bolt(EA)"/>
                    <Input type="Number" value="{path: 'spec>/spare_part_eyebolt_count', type: 'sap.ui.model.type.Integer'}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Ej-Sleeve(EA)"/> 
                    <Input type="Number" value="{path: 'spec>/spare_part_ejslave_count', type: 'sap.ui.model.type.Integer'}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Ejector-Pin(EA)"/> 
                    <Input type="Number" value="{path: 'spec>/spare_part_ejector_pin_count', type: 'sap.ui.model.type.Integer'}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Heater(KIT)"/>
                    <Input type="Number" value="{path: 'spec>/spare_part_heater_count', type: 'sap.ui.model.type.Integer'}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Speaker Core(EA)"/>
                    <Input type="Number" value="{path: 'spec>/spare_part_speaker_core_count', type: 'sap.ui.model.type.Integer'}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">
                    <Label text="Etc.(EA)"/>
                    <Input type="Number" value="{path: 'spec>/spare_part_etc_count', type: 'sap.ui.model.type.Integer'}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                </form:content>
        </form:SimpleForm>

        <form:SimpleForm 
                editable="false"
                layout="ResponsiveGridLayout">
                <form:content>

                <core:Title text="Mold Meterial" />

                <VBox height="3.5rem">

                    <Label text="Cavity/Punch"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/mold_cavity_material_name}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CAVITY'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">

                    <Label text="Core/Die"/> 
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/mold_core_material_name}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CORE'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">

                    <Label text="Slide Core"/> 
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/mold_slide_core_material_name}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_SCORE'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">

                    <Label text="Taper Pin"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/mold_slope_material_name}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_TAPER_PIN'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">

                    <Label text="Top Circle"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/top_circle_material_name}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CORE'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">

                    <Label text="Bottom Circle"/>
                    <Select   width="100%"
                                forceSelection="false"
                                selectedKey="{spec>/bottom_circle_material_name}"
                                items="{
                                            path : 'util>/Code',
                                            filters : [
                                                {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CORE'}
                                            ]
                                        }"
                                editable="false"
                                class="readonlyField"
                    >
                        <core:Item key="{util>code}" text="{util>code_name}" />
                    </Select>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                <VBox height="3.5rem">

                    <Label text="Cycle Time(Sec)"/>
                    <Input value="{spec>/cycle_time}" editable="false" class="readonlyField"/>
                    <layoutData>
                        <l:GridData
                            span="XL4 L4 M4 S6" />
                    </layoutData>
                </VBox>

                

            </form:content>
        </form:SimpleForm>
	</VBox>
</core:FragmentDefinition>