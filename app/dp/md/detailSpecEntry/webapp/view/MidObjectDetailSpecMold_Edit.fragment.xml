<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
    xmlns:uxap="sap.uxap" >
<VBox class="sapUiSmallMargin">
    <form:SimpleForm 
        id="frmMold"
        layout="ResponsiveGridLayout"
         >
        <form:content>
            <VBox height="3.5rem">
                <Label text="Inspection" required="true"/>
                <Switch state="{spec>/inspection_flag}" customTextOn="Yes" customTextOff="No" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Assembly Approval" required="true"/> 
                <Switch state="{spec>/assembly_approval_flag}" customTextOn="Yes" customTextOff="No" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Use Material" required="true"/> 
                <Input value="{spec>/use_material_value}" required="true"/>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Cavity Qty" required="true"/>
                <Input value="{spec>/cavity_process_qty}" required="true"/>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Order Qty"/>
                <Input value="{spec>/order_qty}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Sample Qty"/>
                <Input type="Number" value="{path: 'spec>/mold_sample_qty', type: 'sap.ui.model.type.Integer'}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Demand Life(Shot)" required="true"/>
                <Input type="Number" value="{path: 'spec>/request_life_shot_count', type: 'sap.ui.model.type.Integer'}" required="true"/>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Mold Weight(Kg)" required="true"/>
                <Input value="{spec>/mold_weight}" required="true"/>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Mold Type" required="true"/>
                <Select   width="100%" forceSelection="false"
                            required="true"
                            selectedKey="{master>/mold_type_code}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_TYPE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Hot Runner Valve Qty(EA)" required="true"/>
                <Input type="Number" value="{path: 'spec>/mold_hot_runner_qty', type: 'sap.ui.model.type.Integer'}" required="true" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Hot Runner Maker" required="true"/>
                <Select   width="100%" forceSelection="false"
                            required="true"
                            selectedKey="{spec>/hot_runner_maker_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_HR_MAKER'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Mold Structure" required="true"/>
                <Select   width="100%" forceSelection="false"
                            required="true"
                            selectedKey="{spec>/die_form}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_STRUCTURE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Size" required="true"/>
                <Select   width="100%" forceSelection="false"
                            required="true"
                            selectedKey="{spec>/mold_size}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_SIZE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Mold Size(W*L*H)" required="true"/>
                <Input value="{spec>/mold_size_2}" required="true"/>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Runner"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_hot_runner_spec_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_RUNNER'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Gate"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_gate_spec_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_GATE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Under Cut"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_undercut_spec_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_UNDER_CUT'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Return Type"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_return_type_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_RETURN_TYPE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Form Out"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_formout_method}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_FORM_OUT'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Out Process"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_outform_spec_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_OUT_PROCESS'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Runner Out"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_runnerout_spec_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_RUNNER_OUT'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Use Machine(Ton)"/>
                <Input value="{spec>/mold_tonnage}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

        </form:content>
    </form:SimpleForm>



        <form:SimpleForm 
            
            layout="ResponsiveGridLayout"
            >

            <form:content>

            <core:Title text="Spare Parts" />

            <VBox height="3.5rem">
                <Label text="Eye-Bolt(EA)"/>
                <Input type="Number" value="{path: 'spec>/spare_part_eyebolt_count', type: 'sap.ui.model.type.Integer'}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Ej-Sleeve(EA)"/> 
                <Input type="Number" value="{path: 'spec>/spare_part_ejslave_count', type: 'sap.ui.model.type.Integer'}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Ejector-Pin(EA)"/> 
                <Input type="Number" value="{path: 'spec>/spare_part_ejector_pin_count', type: 'sap.ui.model.type.Integer'}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Heater(KIT)"/>
                <Input type="Number" value="{path: 'spec>/spare_part_heater_count', type: 'sap.ui.model.type.Integer'}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Speaker Core(EA)"/>
                <Input type="Number" value="{path: 'spec>/spare_part_speaker_core_count', type: 'sap.ui.model.type.Integer'}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">
                <Label text="Etc.(EA)"/>
                <Input type="Number" value="{path: 'spec>/spare_part_etc_count', type: 'sap.ui.model.type.Integer'}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            </form:content>
    </form:SimpleForm>

    <form:SimpleForm 
            
            layout="ResponsiveGridLayout">
            <form:content>

            <core:Title text="Mold Meterial" />

            <VBox height="3.5rem">

                <Label text="Cavity/Punch"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_cavity_material_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CAVITY'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">

                <Label text="Core/Die"/> 
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_core_material_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CORE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">

                <Label text="Slide Core"/> 
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_slide_core_material_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_SCORE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">

                <Label text="Taper Pin"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/mold_slope_material_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_TAPER_PIN'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">

                <Label text="Top Circle"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/top_circle_material_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CORE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">

                <Label text="Bottom Circle"/>
                <Select   width="100%" forceSelection="false"
                            selectedKey="{spec>/bottom_circle_material_name}"
                            items="{
                                        path : 'util>/Code',
                                        filters : [
                                            {path : 'tenant_id', operator : 'EQ', value1 : 'L2600'},
                                            {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_MOLD_CORE'}
                                        ]
                                    }"
                            
                            
                >
                    <core:Item key="{util>code}" text="{util>code_name}" />
                </Select>
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            <VBox height="3.5rem">

                <Label text="Cycle Time(Sec)"/>
                <Input value="{spec>/cycle_time}" />
                <layoutData>
                    <l:GridData
                        span="XL4 L4 M4 S6" />
                </layoutData>
            </VBox>

            

        </form:content>
    </form:SimpleForm>
	</VBox>
</core:FragmentDefinition>