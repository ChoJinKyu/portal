<mvc:View controllerName="dp.md.moldReceiptConfirm.controller.moldReceiptConfirm" 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m" 
    xmlns:f="sap.f" 
    xmlns:form="sap.ui.layout.form" 
    xmlns:layout="sap.ui.layout" 
    xmlns:core="sap.ui.core" 
    xmlns:table="sap.ui.table" 
    xmlns:sc="sap.suite.ui.commons" 
    xmlns:microchart="sap.suite.ui.microchart" 
    xmlns:lib-m="ext.lib.m"
    core:require="{ I18NFormatter: 'dp/md/util/controller/I18NFormatter' }"
    height="100%">

    <f:DynamicPage id="page"
        headerExpanded="false" 
        toggleHeaderOnTitleClick="true">
        <!-- DynamicPage Title -->
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="{i18n>appTitle}" />
                </f:heading>
                <f:snappedContent>
                    <HBox>
                        <Text text=""/>
                    </HBox>
                    <HBox>
                        <Title text="Process"/>
                    </HBox>
                    <HBox>
                        <sc:MicroProcessFlow>
                            <sc:content>
                                <sc:MicroProcessFlowItem >
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="PR" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>
    
                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="Budget" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="RFQ" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="Mold Spec" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="Purchase Contract" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="PO" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://busy" color="#F94B50"/>
                                            <Text text="RCV" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://circle-task" color="#BEC9D4"/>
                                            <Text text="Lease Contract" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://circle-task" color="#BEC9D4"/>
                                            <Text text="Invoice" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>
                            </sc:content>
                        </sc:MicroProcessFlow>
                    </HBox>
                    <form:SimpleForm id="pageSearchFormS" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                        <form:content>
                            <VBox>
                                <Label text="{I18N>/COMPANY}" labelFor="searchCompanyS" required="true"></Label>
                                <!--<MultiInput id="searchCompanyS" valueHelpOnly="true" width="100%" valueHelpRequest=".onValueHelpRequested"/>-->
                                <MultiComboBox id="searchCompanyS" width="100%" selectedKey="" required="true"
                                                items="{
                                                    path: 'org>/Company',
                                                    filters: [
                                                        {path: 'tenant_id', operator: 'EQ', value1: 'L2101'}
                                                    ],
                                                    sorter: { path: 'company_code'}
                                                }"
                                selectionFinish="handleSelectionFinishComp">
                                    <items>
                                        <core:Item key="{org>company_code}" text="[{org>company_code}] {org>company_name}" />
                                    </items>
                                </MultiComboBox>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="{I18N>/PLANT}" labelFor="searchDivisionS" required="true"></Label>
                                <MultiComboBox  id="searchDivisionS" width="100%" selectedKey="" required="true"
                                                items="{
                                                    path: '/Divisions',
                                                    filters: [
                                                        {path: 'tenant_id', operator: 'EQ', value1: 'L2101'}
                                                    ],
                                                    sorter: { path: 'org_code'}
                                                }"
                                selectionFinish="handleSelectionFinishDiv" >
                                    <items>
                                        <core:Item key="{org_code}" text="[{org_code}] {org_name}" />
                                    </items>
                                </MultiComboBox>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="{path:'I18N>/CONFIRM_DATE', formatter:'I18NFormatter'}" labelFor="searchCreationDateS"></Label>
                                <DateRangeSelection id="searchCreationDateS" enabled="false" width="100%" change="creationDateChange"  displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD - YYYY-MM-DD"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="{path:'I18N>/STATUS', formatter:'I18NFormatter'}" ></Label>
                                <SegmentedButton id="searchStatusS" selectedKey="" class="sapUiSmallMarginBottom" width="100%" selectionChange="onStatusSelectionChange" >
                                    <items>
                                        <SegmentedButtonItem text="No Confirmed" key="ORD_CRT" width="7em" />
                                        <SegmentedButtonItem text="Confirmed" key="RCV_CNF" width="6em" />
                                        <!-- <SegmentedButtonItem text="All(temp)" key="ALL" width="6em" /> -->
                                    </items>
                                </SegmentedButton>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                        </form:content>
                    </form:SimpleForm>
                </f:snappedContent>
                <f:expandedContent>
                    <HBox>
                        <Text text=""/>
                    </HBox>
                    <HBox>
                        <Title text="Process"/>
                    </HBox>
                    <HBox>
                        <sc:MicroProcessFlow>
                            <sc:content>
                                <sc:MicroProcessFlowItem >
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="PR" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>
    
                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="Budget" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="RFQ" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="Mold Spec" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="Purchase Contract" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://message-success" color="#BEC9D4"/>
                                            <Text text="PO" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://busy" color="#F94B50"/>
                                            <Text text="RCV" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://circle-task" color="#BEC9D4"/>
                                            <Text text="Lease Contract" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>

                                <sc:MicroProcessFlowItem>
                                    <sc:customControl>
                                        <VBox alignItems="Center" width="3.6rem">
                                            <core:Icon src="sap-icon://circle-task" color="#BEC9D4"/>
                                            <Text text="Invoice" textAlign="Center" class=""/>
                                        </VBox>
                                    </sc:customControl>
                                </sc:MicroProcessFlowItem>
                            </sc:content>
                        </sc:MicroProcessFlow>
                    </HBox>
                    <form:SimpleForm id="pageSearchFormE" maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                        <form:content>
                            <VBox>
                                <Label text="{I18N>/COMPANY}" labelFor="searchCompanyE" required="true"></Label>
                                <MultiComboBox id="searchCompanyE" width="100%" selectedKey="" required="true"
                                                items="{
                                                    path: 'org>/Company',
                                                    filters: [
                                                        {path: 'tenant_id', operator: 'EQ', value1: 'L2101'}
                                                    ],
                                                    sorter: { path: 'company_code'}
                                                }"
                                selectionFinish="handleSelectionFinishComp">
                                    <items>
                                        <core:Item key="{org>company_code}" text="[{org>company_code}] {org>company_name}" />
                                    </items>
                                </MultiComboBox>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="{I18N>/PLANT}" labelFor="searchDivisionE" required="true"></Label>
                                <MultiComboBox  id="searchDivisionE" width="100%" selectedKey="" required="true"
                                                items="{
                                                    path: '/Divisions',
                                                    filters: [
                                                        {path: 'tenant_id', operator: 'EQ', value1: 'L2101'}
                                                    ],
                                                    sorter: { path: 'org_code'}
                                                }"
                                selectionFinish="handleSelectionFinishDiv" >
                                    <items>
                                        <core:Item key="{org_code}" text="[{org_code}] {org_name}" />
                                    </items>
                                </MultiComboBox>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="{path:'I18N>/CONFIRM_DATE', formatter:'I18NFormatter'}" labelFor="searchCreationDateE"></Label>
                                    <DateRangeSelection id="searchCreationDateE" enabled="false" width="100%" change="creationDateChange"  displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD - YYYY-MM-DD"/>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                            <VBox>
                                <Label text="{path:'I18N>/STATUS', formatter:'I18NFormatter'}" ></Label>
                                <SegmentedButton id="searchStatusE" selectedKey="" class="sapUiSmallMarginBottom" width="100%" selectionChange="onStatusSelectionChange" >
                                    <items>
                                        <SegmentedButtonItem text="No Confirmed" key="ORD_CRT" width="7em" />
                                        <SegmentedButtonItem text="Confirmed" key="RCV_CNF" width="6em" />
                                    </items>
                                </SegmentedButton>
                                <layoutData>
                                    <layout:GridData span="XL3 L3 M6 S12" />
                                </layoutData>
                            </VBox>
                        </form:content>
                    </form:SimpleForm>
                </f:expandedContent>
                <f:actions>
                    <Button id="pageSearchButton" text="{I18N>/SEARCH}" type="Emphasized" press=".onPageSearchButtonPress" />
                </f:actions>
                <f:navigationActions>
                    <Button icon="sap-icon://unfavorite" type="Transparent" />
                </f:navigationActions>
            </f:DynamicPageTitle>
        </f:title>
        <!-- DynamicPage Header -->
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <form:SimpleForm maxContainerCols="2" editable="true" layout="ResponsiveGridLayout" adjustLabelSpan="false" labelSpanL="4" labelSpanM="4" emptySpanL="0" emptySpanM="0" columnsL="2" columnsM="2">
                    <form:content>
                        <VBox>
                            <Label text="{path:'I18N>/ITEM_TYPE', formatter:'I18NFormatter'}" labelFor="searchItemType"></Label>
                            <MultiComboBox   id="searchItemType" width="100%"
                                        selectedKey=""
                                        items="{
                                                    path : 'util>/Code',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_ITEM_TYPE'}
                                                    ],
                                                    sorter: { path: 'sort_no'}
                                                }"
                            >
                                <core:Item key="{util>code}" text="{util>code_name}" />
                            </MultiComboBox>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" linebreak="true" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="{path:'I18N>/PRODUCTION_TYPE', formatter:'I18NFormatter'}" labelFor="searchProductionType"></Label>
                            <MultiComboBox   id="searchProductionType" width="100%"
                                        selectedKey=""
                                        items="{
                                                    path : 'util>/Code',
                                                    filters : [
                                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                                        {path : 'group_code', operator : 'EQ', value1 : 'DP_MD_PROD_TYPE'}
                                                    ],
                                                    sorter: { path: 'sort_no'}
                                                }"
                            >
                                <core:Item key="{util>code}" text="{util>code_name}" />
                            </MultiComboBox>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="{I18N>/DESCRIPTION}"></Label>
                            <Input id="searchDescription" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="{path:'I18N>/ORDER_SUPPLIER', formatter:'I18NFormatter'}"></Label>

                            <Input id="searchSupplier"
                                    value="" 
                                    showValueHelp="true"
                                    valueHelpOnly="false"
                                    valueHelpRequest=".onSuppValueHelpRequested"
                            />
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        
                        <VBox>
                            <Label text="{I18N>/MODEL}" labelFor="searchModel"></Label>
                            <Input id="searchModel" 
                                    showValueHelp="true"  
                                    valueHelpRequest=".onValueHelpRequested"
                                    suggestionItems="{
                                        path: '/Models',
                                        filters: [
                                            {path: 'tenant_id', operator: 'EQ', value1: 'L2101'}
                                        ],
                                        sorter: { path: 'model' }
                            }">
                                <core:Item key="{model}" text="{model}" />
                            </Input>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" linebreak="true" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="{path:'I18N>/MOLD_NUMBER', formatter:'I18NFormatter'}" labelFor="searchMoldNo"></Label>
                            <Input id="searchMoldNo" 
                                    showValueHelp="true"  
                                    valueHelpRequest=".onValueHelpRequested"
                                    suggestionItems="{
                                        path: '/PartNumbers',
                                        filters: [
                                            {path: 'tenant_id', operator: 'EQ', value1: 'L2101'}
                                        ],
                                        sorter: { path: 'mold_number' }
                            }">
                                <core:Item key="{mold_number}" text="{mold_number}" />
                            </Input>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                        <VBox>
                            <Label text="{path:'I18N>/FAMILY_PART_NUMBER', formatter:'I18NFormatter'}" labelFor="searchFamilyPartNo"></Label>
                            <Input id="searchFamilyPartNo" width="100%"/>
                            <layoutData>
                                <layout:GridData span="XL3 L3 M6 S12" />
                            </layoutData>
                        </VBox>
                    </form:content>
                </form:SimpleForm>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <table:Table id="moldMstTable"
                    selectionMode="MultiToggle"
                    fixedColumnCount="7"
                    visibleRowCountMode="Auto"
                    rows="{list>/MoldMasterSpec}"
                    cellClick=".onCellClick"
                    rowSelectionChange=".onCheck">
                <table:extension>
                    <OverflowToolbar>
                        <Title text="{I18N>/LIST} ({= ${list>/MoldMasterSpec}.length || 0})" level="H2"/>
                        <ToolbarSpacer/>
                        <Button id="moldMstTableConfirmButton" type="Emphasized" text="{path:'I18N>/CONFIRM', formatter:'I18NFormatter'}" press=".onMoldMstTableConfirmButtonPress" />
                        <ToolbarSeparator/>
                        <Button id="moldMstTableExcelButton" icon="sap-icon://excel-attachment" press=".onExportPress"   />
                        <Button icon="sap-icon://action-settings" press=".onMoldMstTablePersoButtonPressed" >
                            <layoutData>
                                <OverflowToolbarLayoutData priority="NeverOverflow" />
                            </layoutData>
                        </Button>
                    </OverflowToolbar>
                </table:extension>                             
                <table:columns>
                    <table:Column id="moldCompanyColumn" width="5em" hAlign="Center">
                        <Label text="{I18N>/COMPANY}"/>           
                        <table:template>
                            <Text id="company_code" text="{list>company_code}"/> 
                        </table:template>
                    </table:Column>
                    <table:Column id="moldDivisionColumn" width="4.5em" hAlign="Center">
                        <Label text="{I18N>/PLANT}"/>
                        <table:template>
                            <Text id="org_code" text="{list>org_code}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldModelColumn" width="9em"  hAlign="Center">
                        <Label text="{I18N>/MODEL}"/>
                        <table:template >
                            <Text id="model" text="{list>model}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldMoldNoColumn" width="11em" hAlign="Center">
                        <Label text="Mold No" wrapping="true"/>
                        <table:template>
                            <Text id="mold_number" text="{list>mold_number}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldSeqColumn" width="3em" hAlign="Center">
                        <Label text="Seq"/>
                        <table:template>
                            <Text id="mold_sequence" text="{list>mold_sequence}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldDescriptionColumn" width="8em">
                        <Label text="{I18N>/DESCRIPTION}"/>
                        <table:template>
                            <Text id="spec_name" text="{list>spec_name}"/>
                        </table:template>
                    </table:Column>
                    <table:Column id="moldStatusColumn" width="8em" hAlign="Center">
                        <Label text="{I18N>/STATUS}"/>
                        <table:template>
                            <Text text="{list>mold_progress_status_name}"/>
                        </table:template>
                    </table:Column>


                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/INSPECTION_YN', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Switch name="{list>/mold_id}" state="{list>inspection_flag}" customTextOn="Yes" customTextOff="No" enabled="false" >
                                <layoutData>
                                    <FlexItemData growFactor="1" />
                                </layoutData>
                            </Switch>
                            <!-- <Text text="{list>inspection_flag}"/> -->
                        </table:template>
                    </table:Column>

                    <table:Column width="9em" hAlign="Center">
                        <Label text="{path:'I18N>/INSPECTION_DATE', formatter:'I18NFormatter'}"  required="true"/>
                        <table:template>
                            <DatePicker value="{list>inspection_date_plan}" valueFormat="yyyyMMdd" displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD" required="true" />
                        </table:template>
                    </table:Column>

                    <table:Column width="9em" hAlign="Center">
                        <Label text="{path:'I18N>/COMPLETE_DATE', formatter:'I18NFormatter'}"  required="true"/>
                        <table:template>
                            <DatePicker value="{list>production_complete_date_plan}" valueFormat="yyyyMMdd" displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD" required="true"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/RCV_DOCUMENT', formatter:'I18NFormatter'}"  required="true"/>
                        <table:template>
                            <core:Icon 
                                src="sap-icon://attachment"
                                press="handleAttachmentPress" >
                                <core:layoutData>
                                    <FlexItemData growFactor="1" />
                                </core:layoutData>
                            </core:Icon>
                        </table:template>
                    </table:Column>

                    <table:Column width="9em" hAlign="Center">
                        <Label text="{path:'I18N>/PROD_SUPPLIER', formatter:'I18NFormatter'}"  required="true"/>
                        <table:template>
                            <!-- <Text text="{list>production_supplier_code}"/> -->
                            <Input id=""
                                    value="{list>production_supplier_code}" 
                                    showValueHelp="true"
                                    valueHelpOnly="true"
                                    valueHelpRequest=".onSuppValueHelpRequested"
                                     required="true"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/ORDER_SUPPLIER', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{list>supplier_code}"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/PRODUCTION_TYPE', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{list>mold_production_type_name}"/>
                            
                        </table:template>
                    </table:Column>

                    <table:Column id="moldItemTypeColumn" width="10em" hAlign="Center">
                        <Label text="{path:'I18N>/ITEM_TYPE', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{list>mold_item_type_name}"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/CURRENCY', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{list>currency_code}"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/ESTIMATE_AMOUNT', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{path:'list>order_confirmed_amount', formatter:'.numberFormatter.toNumberString'}"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/ORDER_AMOUNT', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{path:'list>purchasing_amount', formatter:'.numberFormatter.toNumberString'}"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/RECEIPT_AMOUNT', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{path:'list>receiving_amount', formatter:'.numberFormatter.toNumberString'}"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/CONFIRM_USER', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{list>receipt_confirmed_user_name}"/>
                        </table:template>
                    </table:Column>

                    <table:Column width="8em" hAlign="Center">
                        <Label text="{path:'I18N>/CONFIRM_DATE', formatter:'I18NFormatter'}" />
                        <table:template>
                            <!-- <Text text="{path: 'list>receipt_confirmed_date',
                                  formatter: '.dateFormatter.toDateString'}"/> -->
                            <DatePicker value="{list>receipt_confirmed_date}" valueFormat="yyyyMMdd" displayFormat="yyyy-MM-dd" placeholder="YYYY-MM-DD" editable="false" class="readonlyField"/>
                        </table:template>
                    </table:Column>

                    <table:Column id="moldFamilyPartNo1Column" width="12em" hAlign="Center">
                        <Label text="{path:'I18N>/FAMILY_PART_NUMBER', formatter:'I18NFormatter'}" />
                        <table:template>
                            <Text text="{list>family_part_number_1} {list>family_part_number_2} {list>family_part_number_3} {list>family_part_number_4} {list>family_part_number_5}"/>
                        </table:template>
                    </table:Column>

                </table:columns>
            </table:Table>
        </f:content>
        <!-- DynamicPage Footer -->
        <f:footer>
            <OverflowToolbar>
                <ToolbarSpacer />
        <Button text="{I18N>/SAVE}" type="Emphasized" press=".onPageSaveButtonPress" />
        <Button text="{I18N>/CANCEL}" type="Transparent" press=".onPageCancelEditButtonPress"/>
            </OverflowToolbar>
        </f:footer>
    </f:DynamicPage>
</mvc:View>