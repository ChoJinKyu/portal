            <uxap:ObjectPageSection titleUppercase="false" title="Labor Cost">
				<uxap:subSections>
					<uxap:ObjectPageSubSection >
                        <uxap:blocks>



                            <Table 
                                inset="false" 
                                class="sapFDynamicPageAlignContent" 
                                mode="None"
                                growing="true"
                                growingThreshold="100"
                                itemPress="onItemPress"
                                updateFinished=".onMainTableUpdateFinished"
                                items="{
                                    path : 'rrEst>/RepairCostConfig',
                                    filters : [
                                        {path : 'tenant_id', operator : 'EQ', value1 : 'L2101'},
                                        {path : 'company_code', operator : 'EQ', value1 : 'LGESL'},
                                        {path : 'org_code', operator : 'EQ', value1 : 'A040'},
                                        {path : 'item_type', operator : 'EQ', value1 : 'P'},
                                        {path : 'item_size', operator : 'EQ', value1 : 'S'}
                                    ]
                                }"
                                width="auto">
                                <columns>
                                    <Column >
                                        <Text text="{path:'I18N>/NO', formatter:'I18NFormatter'}" />
                                        <footer>
                                            <Text text="Total(①)" />
                                        </footer>
                                    </Column>
                                    <Column  >
                                        <Text text="{path:'I18N>/PROCESS_NAME', formatter:'I18NFormatter'}"/>
                                    </Column>
                                    <Column  >
                                        <Text text="{path:'I18N>/PREFERENCE', formatter:'I18NFormatter'}" />
                                    </Column>
                                    <Column  >
                                        <Text text="{path:'I18N>/SIZE', formatter:'I18NFormatter'}" />
                                    </Column>
                                    <Column  >
                                        <Text text="{path:'I18N>/PRICE', formatter:'I18NFormatter'}" />
                                    </Column>
                                    <Column  >
                                        <Text text="{path:'I18N>/CURRENCY', formatter:'I18NFormatter'}" />
                                        <footer>
                                            <Text text="KRW" />
                                        </footer>
                                    </Column>
                                    <Column  >
                                        <Text text="{path:'I18N>/ESTIMATION', formatter:'I18NFormatter'} {path:'I18N>/LABOR_HOUR', formatter:'I18NFormatter'}" />
                                    </Column>
                                    <Column  >
                                        <Text text="{path:'I18N>/ESTIMATION', formatter:'I18NFormatter'} {path:'I18N>/AMOUNT', formatter:'I18NFormatter'}" />
                                        <footer>
                                            <Text text="0" />
                                        </footer>
                                    </Column>
                                    <Column >
                                        <Text text="{path:'I18N>/AGREEMENT', formatter:'I18NFormatter'} {path:'I18N>/LABOR_HOUR', formatter:'I18NFormatter'}" />
                                    </Column>
                                    <Column >
                                        <Text text="{path:'I18N>/AGREEMENT', formatter:'I18NFormatter'} {path:'I18N>/AMOUNT', formatter:'I18NFormatter'}" />
                                        <footer>
                                            <Text text="0" />
                                        </footer>
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem >
                                        <cells>
                                            <ObjectIdentifier text="{rrEst>item_sequence}" />
                                            <ObjectIdentifier text="{rrEst>item_name}" />
                                            <ObjectIdentifier text="{rrEst>item_code}" />
                                            <ObjectIdentifier text="{rrEst>item_size}" />
                                            <ObjectIdentifier text="{rrEst>net_price}" />
                                            <ObjectIdentifier text="KRW" />
                                            <Input value="0" />
                                            <ObjectIdentifier text="0" />
                                            <Input value="0" />
                                            <ObjectIdentifier text="0" />
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>

                        </uxap:blocks>
                    </uxap:ObjectPageSubSection >
                </uxap:subSections>
            </uxap:ObjectPageSection>

