<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
    xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
    xmlns:uxap="sap.uxap" >

    <l:VerticalLayout class="sapUiContentPadding">
            <Table id="scheduleTable1S"
                    mode="None"
                    sticky="HeaderToolbar,ColumnHeaders"
                    inset="false"
                    class="sapFDynamicPageAlignContent"
                    width="auto"
                    items= "{schedule>/}"
                    >
                <columns>
                    <Column>
                        <Text text="Type" />
                    </Column>
                    <Column>
                        <Text text="Drawing Consent" />
                    </Column>
                    <Column>
                        <Text text="Settled Drawing" />
                    </Column>
                    <Column>
                        <Text text="1st Prod Date" />
                    </Column>
                    <Column>
                        <Text text="Complete Date" />
                    </Column>
                    <Column>
                        <Text text="Inspection Date" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Text text="{schedule>mold_develope_date_type_code}" />
                            <Text text="{schedule>drawing_agreement_date}" />
                            <Text text="{schedule>drawing_confirmed_date}" />
                            <Text text="{schedule>first_production_date}" />
                            <Text text="{schedule>production_complete_date}" />
                            <Text text="{schedule>inspection_date}" />
                        </cells>
                    </ColumnListItem>
                </items>
                
            </Table>
            
            <Table id="scheduleTable2S"
                    mode="None"
                    sticky="HeaderToolbar,ColumnHeaders"
                    inset="false"
                    class="sapFDynamicPageAlignContent"
                    width="auto"
                    items= "{schedule>/}"
                    >
                <columns>
                    <Column>
                        <Text text="Type" />
                    </Column>
                    <Column>
                        <Text text="1st Production Changed Date" />
                    </Column>
                    <Column>
                        <Text text="2nd Production Changed Date" />
                    </Column>
                    <Column>
                        <Text text="3rd Production Changed Date" />
                    </Column>
                    <Column>
                        <Text text="4th Production Changed Date" />
                    </Column>
                    <Column>
                        <Text text="5th Production Changed Date" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Text text="{schedule>mold_develope_date_type_code}" />
                            <Text text="{schedule/first_production_update_date}" placeholder="YYYY-MM-DD"/>
                            <Text text="{schedule/second_production_update_date}" />
                            <Text text="{schedule/third_production_update_date}" placeholder="YYYY-MM-DD"/>
                            <Text text="{schedule/fourth_production_update_date}" />
                            <Text text="{schedule/fifth_production_update_date}" placeholder="YYYY-MM-DD"/>
                        </cells>
                    </ColumnListItem>
                </items>
                
            </Table>
            </l:VerticalLayout>
	
</core:FragmentDefinition>