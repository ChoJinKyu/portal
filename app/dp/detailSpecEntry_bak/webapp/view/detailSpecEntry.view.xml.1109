<mvc:View 
    controllerName="dp.detailSpecEntry.controller.detailSpecEntry"
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns:core="sap.ui.core" 
    xmlns="sap.m" 
    xmlns:f="sap.f" 
    xmlns:layout="sap.ui.layout" 
    xmlns:table="sap.ui.table" 
    xmlns:form="sap.ui.layout.form" 
    xmlns:microchart="sap.suite.ui.microchart" 
    xmlns:sc="sap.suite.ui.commons" 
    xmlns:smartFilterBar="sap.ui.comp.smartfilterbar"
    xmlns:smartTable="sap.ui.comp.smarttable"
	xmlns:sv="sap.ui.comp.smartvariants"
    class="sapUiSizeCompact"  
    displayBlock="true" 
    height="100%">
    
    <App id="app">
		<f:DynamicPage id="detailSpecEntryPage" headerExpanded="true" toggleHeaderOnTitleClick="true">
			<!-- DynamicPage Title -->
			<f:title>
				<f:DynamicPageTitle>
					<f:heading>
						<Title text="{i18n>appTitle}"/>
					</f:heading>
					<f:expandedContent>
                    <!--<layout:HorizontalLayout allowWrapping="true">-->
                        <HBox>
                            <Text text=""/>
                        </HBox>
                        <HBox>
                            <Title text="Process"/>
                        </HBox>
                        <HBox>
                            <sc:MicroProcessFlow>
                                <sc:content>
                                    <sc:MicroProcessFlowItem state="Error" stepWidth="2rem" icon="sap-icon://circle-task" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                </sc:content>
                            </sc:MicroProcessFlow>
                        </HBox>
                        <HBox>
                            <Text renderWhitespace="true" text=" PR    " />
                            <Text renderWhitespace="true" text=" Budget    " />
                            <Text renderWhitespace="true" text=" RFQ      " />
                            <Text renderWhitespace="true" text="Mold&#10;Spec  "   />
                            <Text renderWhitespace="true" text="Purchase&#10; Contract" />
                            <Text renderWhitespace="true" text="   PO     " />
                            <Text renderWhitespace="true" text="   RCV   " />
                            <Text renderWhitespace="true" text="  Lease&#10;Contract" />
                            <Text renderWhitespace="true" text=" Invoice" />
                        </HBox>
						<form:Form editable="true" width="100%">
                            <form:formContainers>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Affiliate">
                                            <form:fields>	
                                                <MultiInput id="multiInputAffiliate" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Division">
                                            <form:fields>	
                                                <MultiInput id="multiInputDivision" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Receipt Date">
                                            <form:fields>
                                                <DateRangeSelection id="ReceiptDate" change="handleChange"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Status">
                                            <form:fields>
                                                <Button text="All" press="onAll" />
                                                <Button text="Received" press="onReceived" />
                                                <Button text="Unreceived" press="onUnreceived" />
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                            </form:formContainers>
                            <form:layout>
                            <form:ColumnLayout columnsM="2" columnsL="3" columnsXL="4"/>
                            </form:layout>
                        </form:Form>
                        <!--</layout:HorizontalLayout>-->
					</f:expandedContent>
					<f:snappedContent><HBox>
                            <Text text=""/>
                        </HBox>
                        <HBox>
                            <Title text="Process"/>
                        </HBox>
                        <HBox>
                            <sc:MicroProcessFlow>
                                <sc:content>
                                    <sc:MicroProcessFlowItem state="Error" stepWidth="2rem" icon="sap-icon://circle-task" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                    <sc:MicroProcessFlowItem state="None" stepWidth="2rem" />
                                </sc:content>
                            </sc:MicroProcessFlow>
                        </HBox>
                        <HBox>
                            <Text renderWhitespace="true" text=" PR    " />
                            <Text renderWhitespace="true" text=" Budget    " />
                            <Text renderWhitespace="true" text=" RFQ      " />
                            <Text renderWhitespace="true" text="Mold&#10;Spec  "   />
                            <Text renderWhitespace="true" text="Purchase&#10; Contract" />
                            <Text renderWhitespace="true" text="   PO     " />
                            <Text renderWhitespace="true" text="   RCV   " />
                            <Text renderWhitespace="true" text="  Lease&#10;Contract" />
                            <Text renderWhitespace="true" text=" Invoice" />
                        </HBox>
						<form:Form editable="true" width="100%">
                            <form:formContainers>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Affiliate">
                                            <form:fields>	
                                                <MultiInput id="multiInputAffiliate1" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Division">
                                            <form:fields>	
                                                <MultiInput id="multiInputDivision1" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Receipt Date">
                                            <form:fields>
                                                <DateRangeSelection id="ReceiptDate1" change="handleChange"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Status">
                                            <form:fields>
                                                <Button text="All" press="onAll" />
                                                <Button text="Received" press="onReceived" />
                                                <Button text="Unreceived" press="onUnreceived" />
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                            </form:formContainers>
                            <form:layout>
                            <form:ColumnLayout columnsM="2" columnsL="3" columnsXL="4"/>
                            </form:layout>
                        </form:Form>
					</f:snappedContent>
				</f:DynamicPageTitle>
			</f:title>
			<!-- DynamicPage Header -->
			<f:header>
				<f:DynamicPageHeader pinnable="true">
					<layout:HorizontalLayout allowWrapping="true">
						<form:Form editable="true" width="100%">
                            <form:formContainers>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Item Type">
                                            <form:fields>
                                                <MultiInput id="multiInputItemType" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Model">
                                            <form:fields>
                                                <SearchField
                                                    id="searchModel"
                                                    width="100%"
                                                    placeholder="Search"
                                                    enableSuggestions="true"
                                                    search=".onModelSearch"
                                                    suggest=".onModelSuggest"
                                                    suggestionItems="{
                                                        path: '/MoldMasters',
                                                        sorter: { path: 'model' }
                                                    }">
                                                    <SuggestionItem text="{model}" description="{path:'part_no'} {path:'item_id'}" key="{tenent_id}" />
                                                </SearchField>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Production Type">
                                            <form:fields>
                                                <MultiInput id="multiInputProductionType" valueHelpOnly="true" valueHelpRequest=".onValueHelpRequested"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Part No">
                                            <form:fields>
                                                <SearchField id="searchPartNo" width="100%"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="Description">
                                            <form:fields>
                                                <Input id="searchDescription" width="100%"/>
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement label="Family Part No">
                                            <form:fields>
                                                <Input id="searchFamilyPartNo" width="100%"/>
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                                <form:FormContainer>
                                    <form:formElements>
                                        <form:FormElement label="E/D Type">
                                            <form:fields>
                                                <ComboBox id="searchEDType"
                                                    selectedKey=""
                                                    selectionChange="handleSelectionChange"
                                                    selectionFinish="handleSelectionFinish"
                                                    width="350px">
                                                    <core:Item key="D" text="내수"/>
                                                    <core:Item key="E" text="수출"/>                                                         
                                                </ComboBox>
                                                <!--
                                                <ComboBox   id="searchEDType"
                                                            items="{
                                                                        path : '/CodeCombo',
                                                                        filters : [
                                                                            {path : 'tenant_id', operator : 'EQ', value1 : '1000'},
                                                                            {path : 'company_code', operator : 'EQ', value1 : 'G100'},
                                                                            {path : 'group_code', operator : 'EQ', value1 : 'CM_TENANT_CD'},
                                                                            {path : 'language_cd', operator : 'EQ', value1 : 'KO'}                                                                        
                                                                        ]
                                                                    }"
                                                >
                                                    <core:Item key="{code}" text="{code_name}" />
                                                </ComboBox>
                                                -->
                                            </form:fields>
                                        </form:FormElement>
                                        <form:FormElement>
                                            <form:fields>
                                                
                                            </form:fields>
                                        </form:FormElement>
                                    </form:formElements>
                                </form:FormContainer>
                            </form:formContainers>
                            <form:layout>
                            <form:ColumnLayout columnsM="2" columnsL="3" columnsXL="4"/>
                            </form:layout>
                        </form:Form>
					</layout:HorizontalLayout>
                    <HBox width="100%" alignItems="Center" alignContent="End" justifyContent="End">
                        <items>
                            <Button text="Search" type="Emphasized" press="onSearch"/>
                        </items>
                    </HBox>
				</f:DynamicPageHeader>
			</f:header>
			<f:content>
                <Page
                    showHeader="false"
                    enableScrolling="false"
                    class="sapUiContentPadding">
                    
                    <Table 
                        id="moldMstTable" 
                        visibleRowCount="5"
                        items="{
                            path: 'moldMst>/MoldMasters',
                            sorter: { path: 'tenent_id'}
                        }"
                        mode="MultiSelect"
                        updateFinished="onMstUpdateFinished"
                    >
                    
                        <headerToolbar>
                            <Toolbar>
                                <ToolbarSpacer/>
                                <Button text="Bind &amp; Receipt" press=".onBindReceipt" />
                                <Button text="Cancel Bind" press=".onCancelBind" />
                                <Button text="Delete" press=".onDelete" />
                                <Button type="Emphasized" text="Receipt" press=".onReceipt" />
                                <Button icon="sap-icon://excel-attachment" press="onPersoButtonPressed" />
                                <Button icon="sap-icon://action-settings" press="onPersoButtonPressed" >
                                    <layoutData>
                                        <OverflowToolbarLayoutData priority="NeverOverflow" />
                                    </layoutData>
                                </Button>
                            </Toolbar>
                        </headerToolbar>
                        <columns>
                            <Column id="affiliateCol" width="6em" hAlign="Center" >
                                <Text text="Affiliate"/>
                            </Column>
                            <Column id="divisionCol" width="6em" minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                                <Text text="Division"/>
                            </Column>
                            <Column id="modelCol" width="12em" minScreenWidth="Tablet" demandPopin="true" >
                                <Text text="Model"/>
                            </Column>
                            <Column id="partNoCol" width="12em" minScreenWidth="Tablet" demandPopin="true" hAlign="Center">
                                <Text text="Part No"/>
                            </Column>
                            <Column id="seqCol" width="5em" minScreenWidth="Tablet" hAlign="Center">
                                <Text text="Seq"/>
                            </Column>
                            <Column id="descriptionCol" width="12em">
                                <Text text="Description"/>
                            </Column>
                            <Column id="statusCol" width="12em" hAlign="Center">
                                <Text text="Status"/>
                            </Column>
                            <Column id="itemTypeCol" width="10em" hAlign="Center" demandPopin="true" >
                                <Text text="Item&#10;Type"/>
                            </Column>
                            <Column id="requesterCol" width="12em" hAlign="Center">
                                <Text text="Requester"/>
                            </Column>
                            <Column id="moldDeveloperCol" width="12em" hAlign="Center">
                                <Text text="Mold&#10;Developer"/>
                            </Column>
                            <Column id="remarkCol" width="12em" hAlign="Center">
                                <Text text="Remark"/>
                            </Column>
                            <Column id="projectCodeCol" width="12em" hAlign="Center">
                                <Text text="Project Code"/>
                            </Column>
                            <Column id="projectNameCol" width="12em" hAlign="Center">
                                <Text text="Project Name"/>
                            </Column>
                            <Column id="familyPartNo1Col" width="12em" hAlign="Center">
                                <Text text="Family&#10;Part No(1)"/>
                            </Column>
                            <Column id="familyPartNo2Col" width="12em" hAlign="Center">
                                <Text text="Family&#10;Part No(2)"/>
                            </Column>
                            <Column id="familyPartNo3Col" width="12em" hAlign="Center">
                                <Text text="Family&#10;Part No(3)"/>
                            </Column>
                            <Column id="familyPartNo4Col" width="12em" hAlign="Center">
                                <Text text="Family&#10;Part No(4)"/>
                            </Column>
                            <Column id="familyPartNo5Col" width="12em" hAlign="Center">
                                <Text text="Family&#10;Part No(5)"/>
                            </Column>
                        </columns>
                        <items>
                            <ColumnListItem type="Active" id="itemColumnListItem" press="onitemTableItemPress">
                                <cells>
                                    <Text id="company_code" text="{moldMst>company_code}"/>
                                    <Text id="org_code" text="{moldMst>org_code}"/>
                                    <Text id="model" text="{moldMst>model}"/>
                                    <Text id="part_no" text="{moldMst>part_no}"/>
                                    <Text id="item_seq" text="{moldMst>item_seq}"/>
                                    <Text id="description" text="{moldMst>description}"/>
                                    <Text id="order_nego_status" text="{moldMst>order_nego_status}"/>
                                    <Text id="family_flag" text="{moldMst>family_flag}"/>
                                    <Text id="prod_type" text="{moldMst>prod_type}"/>
                                    <Text id="item_type" text="{moldMst>item_type}"/>
                                    <Text id="mold_type" text="{moldMst>mold_type}"/>
                                    <Text id="e_d_type" text="{moldMst>e_d_type}"/>
                                    <Text id="test_use_flag" text="{moldMst>test_use_flag}"/>
                                    <Text id="process_type" text="{moldMst>process_type}"/>
                                    <Text id="mold_structure" text="{moldMst>mold_structure}"/>
                                    <Text id="size" text="{moldMst>size}"/>
                                    <Text id="receipt_date" text="{moldMst>receipt_date}"/>
                                    <Text id="requester" text="{moldMst>requester}"/>
                                    <Text id="mold_developer" text="{moldMst>mold_developer}"/>
                                    <Text id="project_code" text="{moldMst>project_code}"/>
                                    <Text id="remark" text="{moldMst>remark}"/>
                                    <Text id="project_name" text="{moldMst>project_name}"/>
                                    <Text id="familyPartNo1" text="{moldMst>familyPartNo1}"/>
                                    <Text id="familyPartNo2" text="{moldMst>familyPartNo2}"/>
                                    <Text id="familyPartNo3" text="{moldMst>familyPartNo3}"/>
                                    <Text id="familyPartNo4" text="{moldMst>familyPartNo4}"/>
                                    <Text id="familyPartNo5" text="{moldMst>familyPartNo5}"/>
                                </cells>
                            </ColumnListItem>
                        </items>
                    </Table>
                
                </Page>
			</f:content>
		</f:DynamicPage>
    </App>
</mvc:View>