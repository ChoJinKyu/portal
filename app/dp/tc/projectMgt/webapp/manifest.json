{
    "_version": "1.12.0",
    "sap.app": {
        "id": "dp.tc.projectMgt",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "ach": "ach",
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "dataSources": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "cm.codeMgt.i18n.i18n"
                }
            },
            "mainService": {
                "uri": "srv-api/odata/v2/dp.ProjectMgtService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0",
                    "localUri": "localService/metadata.xml"
                }
            },
            "v4Proc": {
                "uri": "srv-api/odata/v4/dp.ProjectMgtService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "v4ProcMcst": {
                "uri": "srv-api/odata/v4/dp.McstProjectMgtV4Service/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            },
            "commonUtilService": {
                "uri": "srv-api/odata/v2/cm.util.CommonService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "orgMgrService": {
                "uri": "srv-api/odata/v2/cm.OrgMgtService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }, 
            "McstProjectMgtService" : {
                "uri": "srv-api/odata/v2/dp.McstProjectMgtService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "McstBomMgtService" : {
                "uri": "srv-api/odata/v2/dp.McstBomMgtService/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "sppCap_ui_dev"
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "dp.tc.projectMgt.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.60.1",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                "bundleName": "dp.tc.projectMgt.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "v4Proc": {
                "dataSource": "v4Proc",
                "preload": false,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": false
                }
            },
            "v4ProcMcst": {
                "dataSource": "v4ProcMcst",
                "preload": false,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": false
                }
            },
            "util": {
                "dataSource": "commonUtilService",
                "preload": true,
                "settings": {
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": false,
                    "useBatch": false
                }
            },
            "org": {
                "dataSource": "orgMgrService",
                "preload": true,
                "settings": {
                "defaultBindingMode": "TwoWay",
                "defaultCountMode": "Inline",
                "refreshAfterChange": true,
                "useBatch": true
                }
            },
            "listModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "detailModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "eventsModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "priceModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "exchangeModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "mcstProjectMgtModel": {
                "dataSource": "McstProjectMgtService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "mcstBomMgtModel": {
                "dataSource": "McstBomMgtService",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$direct",
                    "synchronizationMode": "None",
                    "autoExpandSelect": true
                }
            },
            "partListModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            },
            "popupListModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "resourceRoots": {
            "ext.lib": "../../../../lib"
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "dp.tc.projectMgt.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false,
                "transition": "slide"
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "ProjectMgtList",
                    "target": "ProjectMgtList"
                },
                {
                    "pattern": "ProjectInfo/{tenant_id}/{project_code}/{model_code}",
                    "name": "ProjectInfo",
                    "target": "ProjectInfo"
                },
                {
                    "pattern": "McstProjectInfo/{tenant_id}/{project_code}/{model_code}/{version_number}/{view_mode}",
                    "name": "McstProjectInfo",
                    "target": "McstProjectInfo"
                },
                {
                    "pattern": "McstBomInfo/{tenant_id}/{project_code}/{model_code}/{version_number}/{view_mode}",
                    "name": "McstBomInfo",
                    "target": "McstBomInfo"
                }

            ],
            "targets": {
                "ProjectMgtList": {
                    "viewId": "ProjectMgtList",
                    "viewName": "ProjectMgtList"
                },
                "ProjectInfo": {
                    "viewId": "ProjectInfo",
                    "viewName": "ProjectInfo"
                },
                "McstProjectInfo": {
                    "viewId": "McstProjectInfo",
                    "viewName": "McstProjectInfo"
                },
                "McstBomInfo": {
                    "viewId": "McstBomInfo",
                    "viewName": "McstBomInfo"
                }
            }
        }
    }
}