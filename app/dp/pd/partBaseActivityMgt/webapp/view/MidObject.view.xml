                                    <Column>
                                        <Text text="{I18N>/B_GRADE_STANDARD_DAYS}" />
                                    </Column>
                                    <Column>
                                        <Text text="{I18N>/C_GRADE_STANDARD_DAYS}" />
                                    </Column>
                                    <Column>
                                        <Text text="{I18N>/D_GRADE_STANDARD_DAYS}" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem>
                                        <cells>
                                            <Text text="{category>_row_state_}"/>                                            
                                            <HBox
                                                direction="Column">                                                
                                                <Input
                                                    visible="true"
                                                    editable="false"
                                                    maxLength="30"
                                                    value="{category>category_group_code}" />
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{category>category_group_name}" />
                                                <Input
                                                    visible="{contModel>/editMode}"
                                                    editable="false"
                                                    maxLength="30"
                                                    value="{category>category_group_name}" />
                                            </HBox>
                                            <HBox
                                                direction="Column">                                                
                                                <Input
                                                    visible="true"
                                                    editable="false"
                                                    maxLength="40"
                                                    value="{category>category_code}" />                                                
                                            </HBox>                                            
                                            <HBox
                                                direction="Column">
                                                <Text                                                    
                                                    text="{category>category_name}" />                                                
                                                <Input
                                                    visible="{= ${category>_row_state_} === 'C' }"                                                     
                                                    id="searchCategoryInput"
                                                    placeholder=""
                                                    showValueHelp="true"
                                                    valueHelpRequest=".onDialogCategoryPress"/>
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{= !!${category>active_flag} ? 'Y' : 'N'}"/>
                                                <ToggleButton
                                                    visible="{contModel>/editMode}"                                                    
                                                    text="{= ${category>active_flag}? 'Yes':'No'}" 
                                                    pressed="{category>active_flag}"/>
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{category>s_grade_standard_days}"/>
                                                <Input
                                                    visible="{contModel>/editMode}"
                                                    editable="{contModel>/editMode}"
                                                    maxLength="40"
                                                    value="{category>s_grade_standard_days}"/>
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{category>a_grade_standard_days}"/>
                                                <Input
                                                    visible="{contModel>/editMode}"
                                                    editable="{contModel>/editMode}"
                                                    maxLength="40"
                                                    value="{category>a_grade_standard_days}"/>
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{category>b_grade_standard_days}"/>
                                                <Input
                                                    visible="{contModel>/editMode}"
                                                    editable="{contModel>/editMode}"
                                                    maxLength="40"
                                                    value="{category>b_grade_standard_days}"/>
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{category>c_grade_standard_days}"/>
                                                <Input
                                                    visible="{contModel>/editMode}"
                                                    editable="{contModel>/editMode}"
                                                    maxLength="40"
                                                    value="{category>c_grade_standard_days}"/>
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{category>d_grade_standard_days}"/>
                                                <Input
                                                    visible="{contModel>/editMode}"
                                                    editable="{contModel>/editMode}"
                                                    maxLength="40"
                                                    value="{category>d_grade_standard_days}"/>
