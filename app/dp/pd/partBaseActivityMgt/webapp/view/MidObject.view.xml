                                            id="cateTableDeleteButton" 
                                            type="Transparent" 
                                            text="{I18N>/ROW_DELETE}"
                                            press=".onCateTableDeleteButtonPress"/>                                        
                                    </HBox>
                                </Toolbar>
                            </headerToolbar>
							<columns>
                                    <Column 
                                        visible="{contModel>/editMode}"
                                        width="4em">
                                        <Text text="{I18N>/STATUS}"/>
                                    </Column>
                                    <Column visible="true">
                                        <Text text="{I18N>/GROUP_CODE}" />
                                    </Column>
                                    <Column>
                                        <Text text="{I18N>/GROUP}" />
                                    </Column>
                                    <Column visible="true">
                                        <Text text="{I18N>/CATEGORY_CODE}" />
                                    </Column>
                                    <Column>
                                        <Text text="{I18N>/CATEGORY}" />
                                    </Column>
                                    <Column>
                                        <Text text="{I18N>/USE}" />
                                    </Column>
                                    <Column>
                                        <Text text="{I18N>/S_GRADE_STANDARD_DAYS}" />
