<mvc:View controllerName="dp.pd.partBaseActivityMgt.controller.MidObject"
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:uxap="sap.uxap"
    xmlns:table="sap.ui.table" 
	xmlns:layout="sap.ui.layout" 
    xmlns:form="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
    xmlns:richtexteditor="sap.ui.richtexteditor"
	height="100%">

	<uxap:ObjectPageLayout id="page"
		toggleHeaderOnTitleClick="true"
		alwaysShowContentHeader = "true"
		preserveHeaderStateOnScroll="false"
		headerContentPinnable="true"
		isChildPage="true"
		upperCaseAnchorBar="false"
		showTitleInHeaderContent="true"
		showEditHeaderButton="true" 
		showFooter="true">
		<uxap:headerTitle>
			<uxap:ObjectPageDynamicHeaderTitle>
                <uxap:heading>
                    <HBox alignItems="End">
                        <OverflowToolbarButton id="pageNavBackButton" type="Transparent" icon="sap-icon://nav-back" tooltip="back to the list" press=".handleClose" visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"/>
                        <Title text="{I18N>/PART_BASE_ACTIVITY} {I18N>/DETAIL}" wrapping="true" class="" />
                    </HBox>
                </uxap:heading>				
                <uxap:navigationActions>
                    <OverflowToolbarButton type="Transparent" icon="sap-icon://full-screen" press="handleFullScreen" tooltip="Enter Full Screen Mode" visible="{contModel>/midObject/visibleFullScreenBtn}"/>
                    <OverflowToolbarButton type="Transparent" icon="sap-icon://exit-full-screen" press="handleExitFullScreen" tooltip="Exit Full Screen Mode" visible="{= !${contModel>/midObject/visibleFullScreenBtn}}"/>                    					
                </uxap:navigationActions>
			</uxap:ObjectPageDynamicHeaderTitle>
		</uxap:headerTitle>

        <uxap:headerContent>						
            <FlexBox wrap="Wrap" visible="true">
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<layout:HorizontalLayout class="sapUiSmallMarginBeginEnd">
						<Label text="{I18N>/UPDATE_USER_ID} :"/>
                        <Label text="{master>/update_user_id}" class="sapUiTinyMarginBegin sapUiLargeMarginEnd" design="Bold"/>
					</layout:HorizontalLayout>
				</layout:VerticalLayout>
				<layout:VerticalLayout class="sapUiSmallMarginBeginEnd">
					<layout:HorizontalLayout class="sapUiSmallMarginBeginEnd">
						<Label text="{I18N>/LATEST} {I18N>/CHANGE} {I18N>/DATE_TIME} :" class="sapUiLargeMarginBegin" />
                        <Label text="{
                            path: 'master>/local_update_dtm',
                            formatter: '.dateFormatter.toUTCDateTimeString'
                        }"  
                        class="sapUiTinyMarginBegin"  
                        design="Bold"/>
					</layout:HorizontalLayout>					
				</layout:VerticalLayout>
			</FlexBox>            
		</uxap:headerContent>       

		<uxap:sections>
			<uxap:ObjectPageSection showTitle="false" id="pageSectionMain" titleUppercase="false" title="{I18N>/DEFAULT}{I18N>/INFORMATION}">
				<uxap:subSections>
					<uxap:ObjectPageSubSection id="pageSubSection1">
                        <form:SimpleForm         
                            editable="true"
                            layout="ResponsiveGridLayout"			
                            labelSpanXL="12"
                            labelSpanL="12"
                            labelSpanM="12"
                            labelSpanS="12"
                            adjustLabelSpan="false"
                            emptySpanXL="0"
                            emptySpanL="0"
                            emptySpanM="0"
                            emptySpanS="0"
                            columnsXL="1"
                            columnsL="1"
                            columnsM="1" >
                            
                            <form:content>
                                <VBox width="100%">            
                                    <Label text="{I18N>/PROD_ACTIVITY_CODE}" required="true"/>
                                    <Text
                                        width="50%"
                                        text="{master>/activity_code}"
                                        visible="{contModel>/readMode}">
                                    </Text>
                                    <Input 
                                        width="50%"
                                        value="{
                                            path: 'master>/activity_code',
                                            type: 'sap.ui.model.type.String',
                                            constraints: {
                                                maxLength: 40
                                            }
                                        }" visible="{contModel>/editMode}" editable="false"></Input>
                                        <layoutData>
                                            <layout:GridData
                                                span="XL12 L12 M12 S12" />
                                        </layoutData>
                                </VBox>
                                <VBox width="100%">			
                                    <Label text="{I18N>/DESCRIPTION}"/>
                                    <Text
                                        width="50%"
                                        text="{master>/description}"
                                        visible="{contModel>/readMode}">
                                    </Text>
                                    <Input 
                                        width="50%"
                                        value="{
                                            path: 'master>/description',
                                            type: 'sap.ui.model.type.String',
                                            constraints: {
                                                maxLength: 240
                                            }
                                        }" visible="{contModel>/editMode}" editable="{contModel>/editMode}"></Input>
                                        <layoutData>
                                            <layout:GridData
                                                span="XL12 L12 M12 S12" />
                                        </layoutData>
                                </VBox>
                                <VBox width="100%">			
                                    <Label text="{I18N>/SEQUENCE}"/>
                                    <Text
                                        width="50%"
                                        text="{master>/sequence}"
                                        visible="{contModel>/readMode}">
                                    </Text>
                                    <Input 
                                        width="50%"
                                        value="{
                                            path: 'master>/sequence'
                                        }" type="Number"  visible="{contModel>/editMode}" editable="{= !${contModel>/createMode}}"></Input>
                                        <layoutData>
                                            <layout:GridData
                                                span="XL12 L12 M12 S12" />
                                        </layoutData>
                                </VBox>
                                <VBox>
                                    <Label text="{I18N>/STATUS}"/> 
                                    <Text 
                                        text="{master>/active_flag_val}"
                                        visible="{= !${contModel>/editMode} }"/>                                   
                                    <SegmentedButton 
                                        id="editUseflag"
                                        selectedKey="{master>/active_flag}" 
                                        selectionChange=""
                                        visible="{contModel>/editMode}"                                        
                                        width="100%">
                                        <items>
                                            <SegmentedButtonItem text="Active" width="5em" key="true"/>
                                            <SegmentedButtonItem text="Inactive" width="5em" key="false"/>
                                        </items>
                                    </SegmentedButton>                                        
                                    <layoutData>
                                        <layout:GridData
                                            span="XL12 L12 M12 S12" />
                                    </layoutData>
                                </VBox>                                
                            </form:content>
                        </form:SimpleForm>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>            				

			<uxap:ObjectPageSection 
                titleUppercase="false" 
                title="{I18N>/MULTILINGUAL} ({= (${languages>/PdPartBaseActivityLng})?${languages>/PdPartBaseActivityLng}.length:0})">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
                    <uxap:blocks>
						<Table 
                            id="lngTable" 
                            mode="{= ${contModel>/readMode}?'None':'MultiSelect'}"
                            sticky="HeaderToolbar,ColumnHeaders"
                            inset="false"
                            class="sapFDynamicPageAlignContent"
                            width="auto"
                            items="{
                                path: 'languages>/PdPartBaseActivityLng'
                            }">
							<headerToolbar>
                                <Toolbar visible="{contModel>/editMode}">
                                    <ToolbarSpacer/>
                                    <HBox >
                                        <Button
                                            id="lngTableAddButton" 
                                            type="Transparent" 
                                            text="{I18N>/ROW_ADD}"
                                            press=".onLngTableAddButtonPress"/>
                                        <Button
                                            id="lngTableDeleteButton" 
                                            type="Transparent" 
                                            text="{I18N>/ROW_DELETE}"
                                            press=".onLngTableDeleteButtonPress"/>
                                    </HBox>
                                </Toolbar>
                            </headerToolbar>
							<columns>
                                <Column 
                                    visible="{contModel>/editMode}"
                                    width="4em">
                                    <Text text="{I18N>/STATUS}"/>
                                </Column>
                                <Column>
                                    <Text text="{I18N>/LANGUAGE}" />
                                </Column>
                                <Column>
                                    <Text text="{I18N>/ACTIVITY_NAME}" />
                                </Column>
                            </columns>
                                <items>
                                    <ColumnListItem>
                                        <cells>                                            
                                            <ObjectStatus icon="{
                                                path: 'languages>_row_state_',
                                                formatter: '.formatter.toModelStateColumnIcon'
                                            }" />
                                            <HBox>
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{languages>language_cd}" />                                                
                                                <ComboBox 
                                                    visible="{contModel>/editMode}"
                                                    showSecondaryValues= "true"
                                                    width="100%" 
                                                    selectedKey="{languages>language_cd}"
                                                    items="{
                                                        path: 'util>/Code',
                                                        filters: [
                                                            {path: 'tenant_id', operator: 'EQ', value1: 'L2100'},                                        
                                                            {path: 'group_code', operator: 'EQ', value1: 'CM_LANG_CODE'}
                                                        ]
                                                    }" editable="{= ${languages>_row_state_} === 'C' }" required="true"> 
                                                    <items>
                                                        <core:ListItem 
                                                            key="{util>code}" 
                                                            text="{util>code_name}"
                                                            additionalText="{util>code}"/>
                                                    </items>
                                                </ComboBox>
                                            </HBox>
                                            <HBox
                                                direction="Column">
                                                <Text
                                                    visible="{contModel>/readMode}"
                                                    text="{languages>code_name}" />
                                                <Input
                                                    visible="{contModel>/editMode}"
                                                    maxLength="240"
                                                    value="{languages>code_name}" />
                                            </HBox>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>

            <uxap:ObjectPageSection 
                titleUppercase="false" 
                title="{I18N>/CATEGORY} ({= (${category>/PdPartBaseActivityCategoryView})?${category>/PdPartBaseActivityCategoryView}.length:0})">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
                    <uxap:blocks>
						<table:Table id="cateTable" selectionMode="{= ${contModel>/readMode}?'None':'MultiToggle'}" 
                            cellClick=".onCellClick" rowSelectionChange=".onCheck" enableColumnReordering="false"
                            filter="filterPrice"
                            visibleRowCount="15"
                            rows="{category>/PdPartBaseActivityCategoryView}">
                            <table:extension>
                                <OverflowToolbar visible="{contModel>/editMode}">			
                                    <ToolbarSpacer/>
                                    <Button
                                        id="cateTableBundleAddButton" 
                                        type="Transparent" 
                                        text="일괄 등록"
                                        press="onBundleAddRow"/>
                                    <Button
                                        id="cateTableAddButton" 
                                        type="Transparent" 
                                        text="{I18N>/ROW_ADD}"
                                        press=".onCateAddRow"/>
                                    <Button
                                        id="cateTableDeleteButton" 
                                        type="Transparent" 
                                        text="{I18N>/ROW_DELETE}"
                                        press=".onCateDelRow"/>			
                                    <ToolbarSeparator/>
                                </OverflowToolbar>
                            </table:extension>
                            <table:columns>	                                
                                <table:Column id="state" width="50px" visible="{contModel>/editMode}">
                                <Label text="" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <ObjectStatus icon="{
                                            path: 'category>_row_state_',
                                            formatter: '.formatter.toModelStateColumnIcon'
                                        }" />
                                </table:template>
                                </table:Column>                                
                                <table:Column id="category_group_name" width="100px" filterProperty="category_group_name">
                                <Label text="{I18N>/GROUP}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{category>category_group_name}" textAlign="Center" width="100%" wrapping="false"/>
                                </table:template>
                                </table:Column>                                
                                <table:Column id="category_name_show" visible="{= !${contModel>/editMode}}" filterProperty="category_name">
                                <Label text="{I18N>/CATEGORY}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{category>category_name}" textAlign="Left" width="100%" wrapping="false"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="category_name_edit" visible="{= ${contModel>/editMode}}" filterProperty="category_name">
                                <Label text="{I18N>/CATEGORY}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>                                                                           
                                    <Input
                                        editable="{= ${category>_row_state_} === 'C' }"
                                        id="cateDialog"
                                        valueHelpRequest=".onDialogCategoryPress"
                                        value="{category>category_name}"
                                        showValueHelp="true"
                                        />
                                </table:template>
                                </table:Column>                                
                                <table:Column id="active_flag_show" width="70px" visible="{contModel>/readMode}">
                                <Label text="{I18N>/USE}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{= !!${category>active_flag} ? 'Y' : 'N'}" textAlign="Center" width="100%" wrapping="false"/>                                    
                                </table:template>
                                </table:Column>
                                <table:Column id="active_flag_edit" width="70px" visible="{contModel>/editMode}">
                                <Label text="{I18N>/USE}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>                                    
                                    <ToggleButton                                                                                            
                                        text="{= ${category>active_flag}? 'Yes':'No'}" 
                                        pressed="{category>active_flag}" width="100%"/>
                                </table:template>
                                </table:Column>                                
                                <table:Column id="s_grade_standard_days_show" width="80px" visible="{contModel>/readMode}" filterProperty="s_grade_standard_days">
                                <Label text="{I18N>/S_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{category>s_grade_standard_days}" textAlign="Right" width="100%" wrapping="false"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="s_grade_standard_days_edit" width="100px" visible="{contModel>/editMode}" filterProperty="s_grade_standard_days">
                                <Label text="{I18N>/S_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <!-- <MaskInput
                                        visible="{contModel>/editMode}"
                                        editable="{contModel>/editMode}" 
                                        mask="99999999999" 
                                        placeholderSymbol=" "  
                                        value="{category>s_grade_standard_days}" width="100%" textAlign="Right"  
                                        placeholder="0"/> -->
                                    <Input
                                        visible="{contModel>/editMode}"
                                        editable="{contModel>/editMode}"
                                        maxLength="10"
                                        liveChange=".onInputChange"                                        
                                        value="{category>s_grade_standard_days}" width="100%" textAlign="Right"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="a_grade_standard_days_show" width="80px" visible="{contModel>/readMode}" filterProperty="a_grade_standard_days">
                                <Label text="{I18N>/A_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{category>a_grade_standard_days}" textAlign="Right" width="100%" wrapping="false"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="a_grade_standard_days_edit" width="100px" visible="{contModel>/editMode}" filterProperty="a_grade_standard_days">
                                <Label text="{I18N>/A_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Input
                                        visible="{contModel>/editMode}"
                                        editable="{contModel>/editMode}"
                                        maxLength="10"
                                        liveChange=".onInputChange"
                                        value="{category>a_grade_standard_days}" width="100%" textAlign="Right"/>
                                </table:template>
                                </table:Column>                                
                                <table:Column id="b_grade_standard_days_show" width="80px" visible="{contModel>/readMode}" filterProperty="b_grade_standard_days">
                                <Label text="{I18N>/B_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{category>b_grade_standard_days}" textAlign="Right" width="100%" wrapping="false"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="b_grade_standard_days_edit" width="100px" visible="{contModel>/editMode}" filterProperty="b_grade_standard_days">
                                <Label text="{I18N>/B_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Input
                                        visible="{contModel>/editMode}"
                                        editable="{contModel>/editMode}"
                                        maxLength="10"
                                        liveChange=".onInputChange"
                                        value="{category>b_grade_standard_days}" width="100%" textAlign="Right"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="c_grade_standard_days_show" width="80px" visible="{contModel>/readMode}" filterProperty="c_grade_standard_days">
                                <Label text="{I18N>/C_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{category>c_grade_standard_days}" textAlign="Right" width="100%" wrapping="false"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="c_grade_standard_days_edit" width="100px" visible="{contModel>/editMode}" filterProperty="c_grade_standard_days">
                                <Label text="{I18N>/C_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Input
                                        visible="{contModel>/editMode}"
                                        editable="{contModel>/editMode}"
                                        maxLength="10"
                                        liveChange=".onInputChange"
                                        value="{category>c_grade_standard_days}" width="100%" textAlign="Right"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="d_grade_standard_days_show" width="80px" visible="{contModel>/readMode}" filterProperty="d_grade_standard_days">
                                <Label text="{I18N>/D_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Text text="{category>d_grade_standard_days}" textAlign="Right" width="100%" wrapping="false"/>
                                </table:template>
                                </table:Column>
                                <table:Column id="d_grade_standard_days_edit" width="100px" visible="{contModel>/editMode}" filterProperty="d_grade_standard_days">
                                <Label text="{I18N>/D_GRADE_STANDARD_DAYS}" wrapping="false" textAlign="Center" width="100%"/>
                                <table:template>
                                    <Input
                                        visible="{contModel>/editMode}"
                                        editable="{contModel>/editMode}"
                                        maxLength="10"
                                        liveChange=".onInputChange"
                                        value="{category>d_grade_standard_days}" width="100%" textAlign="Right"/>
                                </table:template>
                                </table:Column>                                
                            </table:columns>
                        </table:Table>
                    </uxap:blocks>
					</uxap:ObjectPageSubSection>
				</uxap:subSections>
			</uxap:ObjectPageSection>
            
            <!-- <uxap:ObjectPageSection 
                titleUppercase="false" 
                title="{I18N>/CATEGORY} ({= (${category>/PdPartBaseActivityCategoryView})?${category>/PdPartBaseActivityCategoryView}.length:0})">
				<uxap:subSections>
					<uxap:ObjectPageSubSection>
                    <uxap:blocks>
						<Table 
                            id="cateTable" 
                            mode="{= ${contModel>/readMode}?'None':'MultiSelect'}"
                            sticky="HeaderToolbar,ColumnHeaders"
                            inset="false"
                            class="sapFDynamicPageAlignContent"
                            width="auto"
                            items="{
                                path: 'category>/PdPartBaseActivityCategoryView'
                            }">
							<headerToolbar>
                                <Toolbar visible="{contModel>/editMode}">
                                    <ToolbarSpacer/>                                   
                                    <HBox >
                                        <Button
                                            id="cateTableBatchAddButton" 
                                            type="Transparent" 
                                            text="일괄 등록"
                                            press=""/>
                                        <Button
                                            id="cateTableAddButton" 
                                            type="Transparent" 
                                            text="{I18N>/ROW_ADD}"
                                            press=".onCateTableAddButtonPress"/>
