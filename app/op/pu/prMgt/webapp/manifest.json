{
  "_version": "1.12.0",
  "sap.app": {
    "id": "op.pu.prMgt",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "ach": "set-ach",
    "resources": "resources.json",
    "dataSources": {
      "mainService": {
        "uri": "srv-api/odata/v2/op.pu.prMgtService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "opUtilTemplateService": {
        "uri": "srv-api/odata/v2/op.util.TemplateService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      },
      "commonUtilService": {
        "uri": "srv-api/odata/v2/cm.util.CommonService/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "op.pu.prMgt.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "handleValidation": true,
    "models": {
        "i18n": {
            "type": "sap.ui.model.resource.ResourceModel",
            "settings": {
                "bundleName": "op.pu.prMgt.i18n.i18n"
            }
        },
        "util": {
            "dataSource": "commonUtilService",
            "preload": true,
            "settings": {
                "defaultBindingMode": "TwoWay",
                "defaultCountMode": "Inline",
                "refreshAfterChange": false,
                "useBatch": true
            }
        },
        "": {
            "dataSource": "mainService",
            "preload": true,
            "settings": {
                "defaultBindingMode": "TwoWay",
                "defaultCountMode": "Inline",
                "refreshAfterChange": true,
                "useBatch": true
            }
        },
        "opUtilTemplateODataModel": {
            "dataSource": "opUtilTemplateService",
            "preload": true,
            "settings": {
                "defaultBindingMode": "TwoWay",
                "defaultCountMode": "Inline",
                "refreshAfterChange": false,
                "useBatch": true
            }
        },            
        "contModel": {
            "type": "sap.ui.model.json.JSONModel",
            "settings": {
                "defaultBindingMode": "TwoWay"
            }
        },
        "prtypeModel": {
            "type": "sap.ui.model.json.JSONModel",
            "settings": {
                "defaultBindingMode": "TwoWay"
            }
        },
        "prtypeModel3": {
            "type": "sap.ui.model.json.JSONModel",
            "settings": {
                "defaultBindingMode": "TwoWay"
            }
        },
            "viewModel": {
                "type": "sap.ui.model.json.JSONModel",
                "settings": {
                    "defaultBindingMode": "TwoWay"
                }
            }
    },
    "resources": {
        "css": [
            {
                "uri": "css/style.css"
            }
        ]
    },
    "resourceRoots": {
      "ext.lib": "../../../../lib",
      "dp": "../../../../dp",
      "cm": "../../../../cm",
      "op": "../../../../op",
      "sp": "../../../../sp"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "op.pu.prMgt.view",
        "controlId": "fcl",
		"transition": "slide",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
          {
              "pattern": ":layout:",
              "name": "mainPage",
              "target": [
                  "mainObject"
              ]
          },
          {
              "pattern": "midView/{layout}/{vMode}/{tenantId}/{company_code}/{pr_number}",
              "name": "midView",
              "target": [
                  "mainObject",
                  "midViewObject"
              ]
          },
          {
              "pattern": "midModify/{layout}/{vMode}/{tenantId}/{company_code}/{pr_number}",
              "name": "midModify",
              "target": [
                  "mainObject",
                  "midModifyObject"
              ]
          },
          {
              "pattern": "midCreate/{layout}/{vMode}/{tenantId}/{company_code}/{pr_type_code}/{pr_type_code_2}/{pr_type_code_3}/{pr_template_number}",
              "name": "midCreate",
              "target": [
                  "mainObject",
                  "midCreateObject"
              ]
          }

      ],
      "targets": {
          "mainObject": {
              "viewName": "MainList",
              "controlAggregation": "beginColumnPages"
          },
          "midViewObject": {
              "viewName": "MidViewObject",
              "controlAggregation": "midColumnPages"
          },
          "midCreateObject": {
              "viewName": "MidCreateObject",
              "controlAggregation": "midColumnPages"
          },
          "midModifyObject": {
              "viewName": "MidCreateObject",
              "controlAggregation": "midColumnPages"
          },
          "notFound": {
            "viewName": "NotFound",
            "viewPath": "ext.lib.view",
            "viewId": "notFound"
          }
      }
    }
  },
  "sap.cloud": {
    "public": true,
    "service": "sppCap_ui_dev"
  }
}