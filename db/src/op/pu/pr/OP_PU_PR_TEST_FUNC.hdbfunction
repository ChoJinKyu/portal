FUNCTION OP_PU_PR_TEST_FUNC (   I_TENANT_ID NVARCHAR(10), 
                                I_PR_NUMBER NVARCHAR(50) )
    RETURNS TABLE(SERVICE_DESC NVARCHAR(100), PR_QUANTITY DECIMAL)
	LANGUAGE SQLSCRIPT
AS
BEGIN 
    RETURN 
        Select TOP 1 SRV.SERVICE_DESC AS SERVICE_DESC
                    , SRV.PR_QUANTITY AS PR_QUANTITY
            FROM OP_PU_PR_SERVICE SRV 
                Where  SRV.PR_NUMBER = :I_PR_NUMBER ;

END ;