FUNCTION XX_SAMPLE_SELECT_FUNC (CD NVARCHAR(5000))
    RETURNS TABLE (ID BIGINT, CD NVARCHAR(5000), NAME NVARCHAR(5000), NAME_CD NVARCHAR(5000))
    LANGUAGE SQLSCRIPT
AS

BEGIN
    RETURN  SELECT  A.ID
                   ,A.CD
                   ,A.NAME
                   ,CASE WHEN A.NAME IS NULL THEN A.CD
                            ELSE A.NAME
                    END AS NAME_CD
            FROM    XX_SAMPLE A
            WHERE   A.CD = :CD;
END