/*SELECT 'PAT' || TO_NCHAR(NOW(), 'YYMMDD') || LPAD(DP_PD_PART_ACTIVITY_SEQ.NEXTVAL, 4, '0') FROM DUMMY;  화면에서 채번시 활용*/
SEQUENCE DP_PD_PART_ACTIVITY_SEQ START WITH 1 MAXVALUE 9999 CYCLE 
RESET BY SELECT IFNULL(MAX(TO_NUMBER(SUBSTRING(ACTIVITY_CODE,9))), 0) + 1
         FROM DP_PD_PART_ACTIVITY_TEMPLATE WHERE ACTIVITY_CODE LIKE 'PAT' || TO_NCHAR(NOW(), 'YYMMDD' ) || '%';