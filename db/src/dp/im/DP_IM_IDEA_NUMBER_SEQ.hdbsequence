/*
  Object : DP_IM_IDEA_NUMBER_SEQ 
  Description : Idea Number Sequence 
  화면에서 Idea Number 채번시 활용
  SELECT TO_NCHAR(NOW(), 'YYMM') || LPAD(DP_IM_IDEA_NUMBER_SEQ.NEXTVAL, 6, '0') FROM DUMMY;
  
*/
SEQUENCE DP_IM_IDEA_NUMBER_SEQ START WITH 1 MAXVALUE 999999 CYCLE 
RESET BY SELECT IFNULL(MAX(TO_NUMBER(SUBSTRING(IDEA_NUMBER,5))), 0) + 1
         FROM DP_IM_SUPPLIER_IDEA WHERE IDEA_NUMBER LIKE TO_NCHAR(NOW(), 'YYMM' ) || '%';