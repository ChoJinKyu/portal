PROCEDURE DP_MD_MST_SET_ID_UPDATE_PROC (
    IN  I_TABLE TABLE(SET_ID NVARCHAR(100))
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 

AS
BEGIN
    DECLARE V_ROWCNT BIGINT;

    SELECT COUNT(*) 
    INTO   V_ROWCNT 
    FROM   :I_TABLE
    ;

    IF (V_ROWCNT > 0) THEN
        UPDATE DP_MD_MST
        SET    SET_ID = '',
               LOCAL_UPDATE_DTM = CURRENT_TIMESTAMP
        WHERE  SET_ID IN (SELECT DISTINCT SET_ID FROM :I_TABLE)
        ;
    END IF;
    
END