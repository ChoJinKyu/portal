FUNCTION DP_MD_GET_COMP_NAME_FUNC ( P_TENANT_ID NVARCHAR(5), P_COMPANY_CODE NVARCHAR(10) )
    RETURNS D_RETURN NVARCHAR(240)
    LANGUAGE SQLSCRIPT
AS

BEGIN

	DECLARE EXIT HANDLER FOR SQLEXCEPTION
	BEGIN
		D_RETURN := NULL;
    END;
    
            SELECT COMPANY_NAME 
            INTO   D_RETURN
            FROM CM_ORG_COMPANY 
            WHERE TENANT_ID = :P_TENANT_ID
            AND COMPANY_CODE = :P_COMPANY_CODE
            ;
END