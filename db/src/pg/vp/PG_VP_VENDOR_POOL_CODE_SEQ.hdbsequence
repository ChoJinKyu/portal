/*SELECT 'VP' || TO_NCHAR(NOW(), 'YYYYMMDD') || LPAD(PG_VP_VENDOR_POOL_CODE_SEQ.NEXTVAL, 4, '0') FROM DUMMY;  화면에서 Vendor Pool 채번시 활용*/
SEQUENCE PG_VP_VENDOR_POOL_CODE_SEQ START WITH 1 MAXVALUE 9999 CYCLE 
RESET BY SELECT IFNULL(MAX(TO_NUMBER(SUBSTRING(VENDOR_POOL_CODE,11))), 0) + 1
         FROM PG_VP_VENDOR_POOL_MST WHERE VENDOR_POOL_CODE LIKE 'VP' || TO_NCHAR(NOW(), 'YYYYMMDD' ) || '%';