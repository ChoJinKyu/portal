/* Vendor poolë³„ Meterial/Supplier Mapping List Title View */
VIEW PG_MD_VP_MATERIAL_MAPP_LIST_TITLE_VIEW (
						IN LANGUAGE_CODE NVARCHAR(4)
						, IN TENANT_ID NVARCHAR(5)
