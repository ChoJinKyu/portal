FUNCTION CM_GET_DEPT_NAME_FUNC ( P_TENANT_ID NVARCHAR(5), P_DEPARTMENT_ID NVARCHAR(100) )
    RETURNS D_RETURN NVARCHAR(240)
    LANGUAGE SQLSCRIPT
AS

BEGIN

	DECLARE EXIT HANDLER FOR SQLEXCEPTION
	BEGIN
		D_RETURN := NULL;
	END;
           SELECT DEPT.DEPARTMENT_LOCAL_NAME
           INTO   D_RETURN
           FROM   CM_HR_DEPARTMENT DEPT
           WHERE  TENANT_ID = :P_TENANT_ID
           AND    DEPARTMENT_ID = :P_DEPARTMENT_ID;
           
END