VIEW CM_CODE_VIEW AS 
SELECT L.TENANT_ID
      ,L.GROUP_CODE
      ,L.CODE
      ,L.CODE_NAME
      ,L.LANGUAGE_CD
FROM   CM_CODE_MST M
JOIN   CM_CODE_DTL D
ON (      M.TENANT_ID = D.TENANT_ID
AND    M.GROUP_CODE = D.GROUP_CODE )
JOIN   CM_CODE_LNG L
ON ( D.TENANT_ID = L.TENANT_ID 
AND D.GROUP_CODE = L.GROUP_CODE
AND D.CODE = L.CODE
)
