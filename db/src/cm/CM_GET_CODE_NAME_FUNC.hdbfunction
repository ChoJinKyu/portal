FUNCTION CM_GET_CODE_NAME_FUNC (CD NVARCHAR(5000))
    RETURNS TABLE (MASTER_ID BIGINT, CD NVARCHAR(5000), NAME NVARCHAR(5000))
    LANGUAGE SQLSCRIPT
AS

BEGIN
    RETURN  SELECT  A.MASTER_ID
                   ,A.CD
                   ,A.NAME
            FROM    XX_SAMPLE_MASTER A
            WHERE   A.CD = :CD;
END