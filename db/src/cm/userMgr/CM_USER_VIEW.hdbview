VIEW CM_USER_VIEW AS
SELECT
    U.USER_ID
    , U.USER_NAME
    , U.TENANT_ID
    , U.COMPANY_CODE
    , U.EMPLOYEE_NUMBER
    , U.EMPLOYEE_NAME
    , U.ENGLISH_EMPLOYEE_NAME
    , U.EMPLOYEE_STATUS_CODE
    , U.LANGUAGE_CODE
    , U.TIMEZONE_CODE
    , U.DATE_FORMAT_TYPE_CODE
    , U.DIGITS_FORMAT_TYPE_cODE
    , U.CURRENCY_CODE
    , U.PASSWORD
    , U.EMAIL
    , U.START_DATE
    , U.END_DATE
    , U.USE_FLAG
FROM CM_USER U