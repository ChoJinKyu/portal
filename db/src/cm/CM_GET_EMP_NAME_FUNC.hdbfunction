FUNCTION CM_GET_EMP_NAME_FUNC ( P_TENANT_ID NVARCHAR(5), P_EMPLOYEE_NUMBER NVARCHAR(30) )
    RETURNS D_RETURN NVARCHAR(240)
    LANGUAGE SQLSCRIPT
AS

BEGIN

	DECLARE EXIT HANDLER FOR SQLEXCEPTION
	BEGIN
		D_RETURN := NULL;
	END;
           SELECT EMP.USER_LOCAL_NAME
           INTO   D_RETURN
           FROM   CM_HR_EMPLOYEE EMP
           WHERE  TENANT_ID = :P_TENANT_ID
           AND    EMPLOYEE_NUMBER = :P_EMPLOYEE_NUMBER
           
END