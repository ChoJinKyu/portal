VIEW XX_SAMPLE_SESSION_VIEW AS
SELECT A.TENANT_ID
      ,A.GROUP_CODE
      ,A.LANGUAGE_CD
      ,A.CODE
      ,A.CODE_NAME
      ,A.PARENT_GROUP_CODE
      ,A.PARENT_CODE
      ,A.SORT_NO
FROM   CM_CODE_VIEW A
JOIN   CM_SPP_USER_SESSION_VIEW B ON (A.TENANT_ID = B.TENANT_ID AND A.LANGUAGE_CD = B.LANGUAGE_CODE)
WHERE  A.GROUP_CODE = 'CM_APPROVER_TYPE'