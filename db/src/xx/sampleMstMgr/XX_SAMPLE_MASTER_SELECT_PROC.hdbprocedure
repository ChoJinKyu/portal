PROCEDURE XX_SAMPLE_MASTER_SELECT_PROC (
    IN NAME NVARCHAR(5000),
    OUT O_TABLE TABLE(MASTER_ID BIGINT, CD NVARCHAR(5000), NAME NVARCHAR(5000))
)
LANGUAGE SQLSCRIPT
SQL SECURITY INVOKER 

AS
BEGIN
    O_TABLE =   SELECT  A.MASTER_ID
                       ,A.CD
                       ,A.NAME
                FROM    XX_SAMPLE_MASTER A
                where   a.name like '%' || :NAME || '%';

END