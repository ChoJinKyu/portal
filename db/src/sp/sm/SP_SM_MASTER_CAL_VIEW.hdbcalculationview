      <viewAttribute id="CREATE_USER_ID"/>
      <viewAttribute id="UPDATE_USER_ID"/>
      <viewAttribute id="SYSTEM_CREATE_DTM"/>
      <viewAttribute id="SYSTEM_UPDATE_DTM"/>
    </viewAttributes>
    <calculatedViewAttributes/>
    <input node="CM_CODE_DTL"/>
  </calculationView>
  <calculationView xsi:type="Calculation:ProjectionView" id="code_language" filterExpressionLanguage="COLUMN_ENGINE">
    <viewAttributes>
      <viewAttribute id="TENANT_ID"/>
      <viewAttribute id="GROUP_CODE"/>
      <viewAttribute id="CODE"/>
      <viewAttribute id="LANGUAGE_CD"/>
      <viewAttribute id="CODE_NAME"/>
      <viewAttribute id="LOCAL_CREATE_DTM"/>
      <viewAttribute id="LOCAL_UPDATE_DTM"/>
      <viewAttribute id="CREATE_USER_ID"/>
      <viewAttribute id="UPDATE_USER_ID"/>
      <viewAttribute id="SYSTEM_CREATE_DTM"/>
      <viewAttribute id="SYSTEM_UPDATE_DTM"/>
    </viewAttributes>
    <calculatedViewAttributes/>
    <input node="CM_CODE_LNG"/>
    <filter>&quot;LANGUAGE_CD&quot;=upper(midstr('??LOCALE??',1,2))</filter>
  </calculationView>
  <calculationView xsi:type="Calculation:JoinView" id="join_master_detail" cardinality="C1_1" joinType="inner">
    <viewAttributes>
      <viewAttribute id="TENANT_ID"/>
      <viewAttribute id="CHAIN_GROUP_CODE"/>
      <viewAttribute id="CHAIN_CODE"/>
      <viewAttribute id="GROUP_CODE"/>
      <viewAttribute id="GROUP_NAME"/>
      <viewAttribute id="GROUP_DESCRIPTION"/>
      <viewAttribute id="CODE"/>
      <viewAttribute id="SORT_NO"/>
