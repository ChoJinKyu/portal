      <viewAttribute id="UPDATE_USER_ID"/>
      <viewAttribute id="SYSTEM_CREATE_DTM"/>
      <viewAttribute id="SYSTEM_UPDATE_DTM"/>
    </viewAttributes>
    <calculatedViewAttributes>
      <calculatedViewAttribute id="CHAIN_GROUP_CODE" datatype="NVARCHAR" length="5" expressionLanguage="COLUMN_ENGINE">
        <formula>leftstr(&quot;GROUP_CODE&quot;, 5)</formula>
      </calculatedViewAttribute>
    </calculatedViewAttributes>
    <input node="CM_CODE_MST"/>
    <filter>(&quot;CHAIN_CODE&quot; = 'SP') and (&quot;CHAIN_GROUP_CODE&quot; = 'SP_SM')</filter>
  </calculationView>
  <calculationView xsi:type="Calculation:ProjectionView" id="code_detail">
    <viewAttributes>
