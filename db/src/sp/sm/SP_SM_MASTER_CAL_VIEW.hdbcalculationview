      <mapping xsi:type="Calculation:AttributeMapping" target="CHAIN_GROUP_CODE" source="CHAIN_GROUP_CODE"/>
    </input>
    <input node="join_language">
      <mapping xsi:type="Calculation:AttributeMapping" target="CODE" source="CODE"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="SORT_NO" source="SORT_NO"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="LANGUAGE_CD" source="LANGUAGE_CD"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="CODE_NAME" source="CODE_NAME"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="TENANT_ID" source="TENANT_ID"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="GROUP_CODE" source="GROUP_CODE"/>
    </input>
    <joinAttribute name="TENANT_ID"/>
    <joinAttribute name="GROUP_CODE"/>
  </calculationView>
  <calculationView xsi:type="Calculation:JoinView" id="join_language" cardinality="CN_1" joinType="leftOuter" optimizeJoinColumns="false">
    <viewAttributes>
      <viewAttribute id="CODE"/>
      <viewAttribute id="SORT_NO"/>
      <viewAttribute id="LANGUAGE_CD"/>
      <viewAttribute id="CODE_NAME"/>
    
      <viewAttribute id="TENANT_ID"/>
      <viewAttribute id="GROUP_CODE"/></viewAttributes>
    <calculatedViewAttributes/>
    <input node="code_detail">
      <mapping xsi:type="Calculation:AttributeMapping" target="TENANT_ID" source="TENANT_ID"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="GROUP_CODE" source="GROUP_CODE"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="CODE" source="CODE"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="SORT_NO" source="SORT_NO"/></input>
    <input node="code_language">
      <mapping xsi:type="Calculation:AttributeMapping" target="LANGUAGE_CD" source="LANGUAGE_CD"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="CODE_NAME" source="CODE_NAME"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="TENANT_ID" source="TENANT_ID"/>
      <mapping xsi:type="Calculation:AttributeMapping" target="GROUP_CODE" source="GROUP_CODE"/>
