{
  "build": {
    "target": ".",
    "tasks": [
      {
        "for": "java-cf",
        "src": "srv",
        "options": {
          "model": [
            "./"
          ]
        }
      },
      {
        "for": "hana",
        "src": "db",
        "options": {
          "model": [
            "./"
          ]
        }
      }
    ]
  },
  "hana": {
    "deploy-format": "hdbtable"
  },
  "requires": {
    "db": {
      "kind": "sql"
    }
  },
  "odata": {
    "version": "v4"
  }
}
